#: pllyrics/cog.py:49
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Εμφάνιση της έκδοσης του Cog και των εξαρτήσεών του PyLav"

#: pllyrics/cog.py:65
msgid "Library / Cog"
msgstr "Βιβλιοθήκη / Γρανάζι"

#: pllyrics/cog.py:66
msgid "Version"
msgstr "Εκδοχή"

#: pllyrics/cog.py:81
#, docstring
msgid "Set the lyrics settings"
msgstr "Ορίστε τις ρυθμίσεις στίχων"

#: pllyrics/cog.py:85
#, docstring
msgid "Toggle timed lyrics"
msgstr "Εναλλαγή συγχρονισμένων στίχων"

#: pllyrics/cog.py:93
msgid "Timed lyrics are now {enabled_or_disabled_value}"
msgstr "Οι χρονομετρημένοι στίχοι είναι τώρα {enabled_or_disabled_value}"

#: pllyrics/cog.py:94 pllyrics/cog.py:117
msgid "disabled"
msgstr "απενεργοποιημένο"

#: pllyrics/cog.py:96 pllyrics/cog.py:119
msgid "enabled"
msgstr "ενεργοποιημένο"

#: pllyrics/cog.py:108
#, docstring
msgid "Show full lyrics on track start timed lyrics"
msgstr "Εμφάνιση πλήρων στίχων κατά την έναρξη των συγχρονισμένων στίχων"

#: pllyrics/cog.py:116
msgid "Sending full lyrics on track start {state_value}"
