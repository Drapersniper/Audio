#: pllyrics/cog.py:133
#, docstring
msgid "Set the lyrics channel"
msgstr "Ορίστε το κανάλι στίχων"

#: pllyrics/cog.py:138
msgid "Lyrics channel set to {channel_value}"
msgstr "Το κανάλι στίχων έχει οριστεί σε {channel_value}"

#: pllyrics/cog.py:147
msgid "I will not send lyrics to any channel"
msgstr "Δεν θα στείλω στίχους σε κανένα κανάλι"

#: pllyrics/cog.py:156
#, docstring
msgid "Show lyrics for current track if available"
msgstr "Εμφάνιση στίχων για το τρέχον κομμάτι εάν είναι διαθέσιμο"

#: pllyrics/cog.py:165
msgid "Nothing is currently playing"
msgstr "Δεν παίζει τίποτα αυτήν τη στιγμή"

#: pllyrics/cog.py:204
msgid "Error: {error_value}"
msgstr "Λάθος: {error_value}"

#: pllyrics/cog.py:214
msgid "API Error: {error_value}"
