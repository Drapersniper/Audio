#: plnodes/cog.py:33
#, docstring
msgid "Manage the nodes used by PyLav"
msgstr "Tvarkykite PyLav naudojamus mazgus"

#: plnodes/cog.py:44
#, docstring
msgid "Configure PyLav Nodes"
msgstr "Konfigūruokite PyLav mazgus"

#: plnodes/cog.py:48
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Rodyti krumpliaračio versiją ir jo priklausomybes nuo PyLav"

#: plnodes/cog.py:64
msgid "Library / Cog"
msgstr "Biblioteka / krumpliaratis"

#: plnodes/cog.py:65
msgid "Version"
msgstr "Versija"

#: plnodes/cog.py:78
#, docstring
msgid "Add a node to PyLav"
msgstr "Pridėkite mazgą prie PyLav"

#: plnodes/cog.py:88
msgid "Let's add a node to PyLav!"
msgstr "Pridėkime mazgą prie PyLav!"

#: plnodes/cog.py:89
msgid "(**1**){space_value} - Apply changes and add the node to PyLav.\n"
"(**2**){space_value} - Cancel any changes made and close the menu.\n"
"(**3**){space_value} - Toggle between search-only and search and playback modes.\n"
"(**4**){space_value} - Toggle between SSL on and off.\n"
"(**5**){space_value} - Add a name to the node.\n"
"(**6**){space_value} - Add the host address of the node.\n"
"(**7**){space_value} - Add the port the node uses.\n"
"(**8**){space_value} - Set the password for the node.\n"
"(**9**){space_value} - Set the connection timeout.\n"
"(**10**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space_value} - Taikykite pakeitimus ir pridėkite mazgą prie PyLav.\n"
"(**2**){space_value} - Atšaukite visus atliktus pakeitimus ir uždarykite meniu.\n"
"(**3**){space_value} - Perjunkite tik paieškos ir paieškos bei atkūrimo režimus.\n"
"(**4**){space_value} - Perjungti SSL įjungimą ir išjungimą.\n"
"(**5**){space_value} - Pridėkite pavadinimą prie mazgo.\n"
"(**6**){space_value} - Pridėkite mazgo pagrindinio kompiuterio adresą.\n"
"(**7**){space_value} - Pridėkite mazgo naudojamą prievadą.\n"
"(**8**){space_value} - Nustatykite mazgo slaptažodį.\n"
"(**9**){space_value} - Nustatykite ryšio skirtąjį laiką.\n"
"(**10**) - Pasirinkite, kuriuos šaltinius išjungti šiam mazgui (galima pasirinkti kelis).\n"
"Jei kelis kartus sąveikausite su mygtuku, įsigalios tik paskutinė sąveika.\n\n\n"

#: plnodes/cog.py:137
msgid "Added node {name_value} with the following settings:\n"
"Host: {host_value}\n"
"Port: {port_value}\n"
"Password: {password_value}\n"
"Resume Timeout: {resume_timeout_value}\n"
"Search Only: {search_only_value}\n"
"SSL: {ssl_value}\n"
"Disabled Sources: {disabled_sources_value}\n"
msgstr "Pridėtas mazgas {name_value} su šiais parametrais:\n"
"Priimančiosios: {host_value}\n"
"Uostas: {port_value}\n"
"Slaptažodis: {password_value}\n"
"Tęsti skirtąjį laiką: {resume_timeout_value}\n"
"Tik ieškokite: {search_only_value}\n"
"SSL: {ssl_value}\n"
"Neįgalūs šaltiniai: {disabled_sources_value}\n"

#: plnodes/cog.py:168 plnodes/cog.py:175
msgid "Unable to add this node"
msgstr "Nepavyksta pridėti šio mazgo"

#: plnodes/cog.py:181
#, docstring
msgid "Remove a node from a PyLav instance"
msgstr "Pašalinkite mazgą iš PyLav egzemplioriaus"

#: plnodes/cog.py:193
msgid "{name_value} is managed by PyLav and cannot be removed."
msgstr "{name_value} valdo PyLav ir negali būti pašalintas."

#: plnodes/cog.py:210
msgid "Removed node {name_value}.\n\n"
"{data_value}"
msgstr "Pašalintas mazgas {name_value}.\n\n"
"{data_value}"

#: plnodes/cog.py:219
msgid "Removed node {name_value}, a direct message was sent to you with the node details in case you wish to re-add it"
msgstr "Pašalintas mazgas {name_value}, jums buvo išsiųstas tiesioginis pranešimas su išsamia mazgo informacija, jei norite jį vėl pridėti"

#: plnodes/cog.py:229
#, docstring
msgid "Manage all nodes in a PyLav instance"
msgstr "Valdykite visus \"PyLav\" egzemplioriaus mazgus"

#: plnodes/cog.py:241
msgid "Let's manage some nodes!"
msgstr "Valdykime kai kuriuos mazgus!"

#: plnodes/cog.py:242
msgid "(**1**){space_value} - Cancel any changes made and close the menu.\n"
"(**6**){space_value} - Show sources enabled for this node.\n"
"(**7**){space_value} - Apply changes and add the node to PyLav.\n"
"(**8**){space_value} - Toggle between search-only and search and playback modes.\n"
"(**9**){space_value} - Toggle between SSL on and off.\n"
"(**10**) - Add a name to the node.\n"
"(**11**) - Add the host address of the node.\n"
"(**12**) - Add the port the node uses.\n"
"(**13**) - Set the password for the node.\n"
"(**14**) - Set the connection timeout.\n"
"(**15**) - Remove this node.\n"
"(**16**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space_value} - Atšaukite visus atliktus pakeitimus ir uždarykite meniu.\n"
"(**6**){space_value} - Rodyti šiam mazgui įjungtus šaltinius.\n"
"(**7**){space_value} - Taikykite pakeitimus ir pridėkite mazgą prie PyLav.\n"
"(**8**){space_value} - Perjunkite tik paieškos ir paieškos bei atkūrimo režimus.\n"
"(**9**){space_value} - Perjungti SSL įjungimą ir išjungimą.\n"
"(**10**) - Pridėkite pavadinimą prie mazgo.\n"
"(**11**) - Pridėkite mazgo pagrindinio kompiuterio adresą.\n"
"(**12**) - Pridėkite mazgo naudojamą prievadą.\n"
"(**13**) - Nustatykite mazgo slaptažodį.\n"
"(**14**) - Nustatykite ryšio skirtąjį laiką.\n"
"(**15**) - Pašalinkite šį mazgą.\n"
"(**16**) - Pasirinkite, kuriuos šaltinius išjungti šiam mazgui (galima pasirinkti kelis).\n"
"Jei kelis kartus sąveikausite su mygtuku, įsigalios tik paskutinė sąveika.\n\n\n"

#: plnodes/cog.py:271
msgid "{name_value} is managed by PyLav and cannot be modified with this command."
msgstr "{name_value} valdo PyLav ir negali būti modifikuojamas naudojant šią komandą."

#: plnodes/cog.py:283
msgid "Removed node {name_value}"
msgstr "Pašalintas mazgas {name_value}"

