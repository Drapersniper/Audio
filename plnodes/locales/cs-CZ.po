#: plnodes/cog.py:33
#, docstring
msgid "Manage the nodes used by PyLav"
msgstr "Správa uzlů používaných PyLav"

#: plnodes/cog.py:44
#, docstring
msgid "Configure PyLav Nodes"
msgstr "Konfigurace PyLav uzlů"

#: plnodes/cog.py:48
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Zobrazení verze ozubeného kola a jeho závislostí PyLav"

#: plnodes/cog.py:64
msgid "Library / Cog"
msgstr "Knihovna / Cog"

#: plnodes/cog.py:65
msgid "Version"
msgstr "Verze"

#: plnodes/cog.py:78
#, docstring
msgid "Add a node to PyLav"
msgstr "Přidat uzel do PyLav"

#: plnodes/cog.py:88
msgid "Let's add a node to PyLav!"
msgstr "Pojďme přidat do PyLav!"

#: plnodes/cog.py:89
msgid "(**1**){space_value} - Apply changes and add the node to PyLav.\n"
"(**2**){space_value} - Cancel any changes made and close the menu.\n"
"(**3**){space_value} - Toggle between search-only and search and playback modes.\n"
"(**4**){space_value} - Toggle between SSL on and off.\n"
"(**5**){space_value} - Add a name to the node.\n"
"(**6**){space_value} - Add the host address of the node.\n"
"(**7**){space_value} - Add the port the node uses.\n"
"(**8**){space_value} - Set the password for the node.\n"
"(**9**){space_value} - Set the connection timeout.\n"
"(**10**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space_value} - Aplikujte změny a přidejte uzel do PyLav.\n"
"(**2**){space_value} - Zrušte všechny provedené změny a zavřete nabídku.\n"
"(**3**){space_value} - Přepínat mezi režimy vyhledávání a vyhledáváním a přehráváním.\n"
"(**4**){space_value} - Přepínat mezi SSL zapnuto a vypnuto.\n"
"(**5**){space_value} - Přidejte název uzlu.\n"
"(**6**){space_value} - Přidejte adresu hostitele uzlu.\n"
"(**7**){space_value} - Přidejte port, který uzel používá.\n"
"(**8**){space_value} - Nastavte heslo pro uzel.\n"
"(**9**){space_value} - Nastavte časový limit připojení.\n"
"(**10**) – Vyberte, které zdroje chcete pro tento uzel zakázat (lze vybrat více zdrojů).\n"
"Pokud s tlačítkem interagujete vícekrát, projeví se pouze poslední interakce.\n\n\n"

#: plnodes/cog.py:137
msgid "Added node {name_value} with the following settings:\n"
"Host: {host_value}\n"
"Port: {port_value}\n"
"Password: {password_value}\n"
"Resume Timeout: {resume_timeout_value}\n"
"Search Only: {search_only_value}\n"
"SSL: {ssl_value}\n"
"Disabled Sources: {disabled_sources_value}\n"
msgstr "Přidán uzel {name_value} s následujícím nastavením:\n"
"Hostitel: {host_value}\n"
"Přístav: {port_value}\n"
"Heslo: {password_value}\n"
"Časový limit obnovení: {resume_timeout_value}\n"
"Pouze vyhledávání: {search_only_value}\n"
"SSL: {ssl_value}\n"
"Zakázané zdroje: {disabled_sources_value}\n"

#: plnodes/cog.py:168 plnodes/cog.py:175
msgid "Unable to add this node"
msgstr "Nelze přidat tento uzel"

#: plnodes/cog.py:181
#, docstring
msgid "Remove a node from a PyLav instance"
msgstr "Odstranit uzel z instance PyLav"

#: plnodes/cog.py:193
msgid "{name_value} is managed by PyLav and cannot be removed."
msgstr "{name_value} je spravován PyLav a nelze jej odebrat."

#: plnodes/cog.py:210
msgid "Removed node {name_value}.\n\n"
"{data_value}"
msgstr "Odstraněný uzel {name_value}.\n\n"
"{data_value}"

#: plnodes/cog.py:219
msgid "Removed node {name_value}, a direct message was sent to you with the node details in case you wish to re-add it"
msgstr "Odstraněný uzel {name_value}, byla vám zaslána přímá zpráva s podrobnostmi o uzlu pro případ, že byste jej chtěli znovu přidat"

#: plnodes/cog.py:229
#, docstring
msgid "Manage all nodes in a PyLav instance"
msgstr "Správa všech uzlů v instanci PyLav"

#: plnodes/cog.py:241
msgid "Let's manage some nodes!"
msgstr "Pojďme spravovat nějaké uzly!"

#: plnodes/cog.py:242
msgid "(**1**){space_value} - Cancel any changes made and close the menu.\n"
"(**6**){space_value} - Show sources enabled for this node.\n"
"(**7**){space_value} - Apply changes and add the node to PyLav.\n"
"(**8**){space_value} - Toggle between search-only and search and playback modes.\n"
"(**9**){space_value} - Toggle between SSL on and off.\n"
"(**10**) - Add a name to the node.\n"
"(**11**) - Add the host address of the node.\n"
"(**12**) - Add the port the node uses.\n"
"(**13**) - Set the password for the node.\n"
"(**14**) - Set the connection timeout.\n"
"(**15**) - Remove this node.\n"
"(**16**) - Select which sources to disable for this node (Multiple can be selected).\n"
"If you interact with a button multiple times, only the last interaction will take effect.\n\n\n"
msgstr "(**1**){space_value} - Zrušte všechny provedené změny a zavřete nabídku.\n"
"(**6**){space_value} - Zobrazit zdroje povolené pro tento uzel.\n"
"(**7**){space_value} - Aplikujte změny a přidejte uzel do PyLav.\n"
"(**8**){space_value} - Přepínat mezi režimy vyhledávání a vyhledáváním a přehráváním.\n"
"(**9**){space_value} - Přepínat mezi SSL zapnuto a vypnuto.\n"
"(**10**) – Přidejte název uzlu.\n"
"(**11**) – Přidejte adresu hostitele uzlu.\n"
"(**12**) – Přidejte port, který uzel používá.\n"
"(**13**) – Nastavte heslo pro uzel.\n"
"(**14**) – Nastavte časový limit připojení.\n"
"(**15**) – Odeberte tento uzel.\n"
"(**16**) – Vyberte, které zdroje chcete pro tento uzel zakázat (lze vybrat více zdrojů).\n"
"Pokud s tlačítkem interagujete vícekrát, projeví se pouze poslední interakce.\n\n\n"

#: plnodes/cog.py:271
msgid "{name_value} is managed by PyLav and cannot be modified with this command."
msgstr "{name_value} je spravován pomocí PyLav a nelze jej upravit pomocí tohoto příkazu."

#: plnodes/cog.py:283
msgid "Removed node {name_value}"
msgstr "Odstraněný uzel {name_value}"

#: plnodes/cog.py:319
msgid "Changed node {name_value} to the following settings:\n"
"Host: {host_value}\n"
"Port: {port_value}\n"
"Password: {password_value}\n"
"Resume Timeout: {resume_timeout_value}\n"
"Search Only: {search_only_value}\n"
"SSL: {ssl_value}\n"
"Disabled Sources: {disabled_sources_value}\n"
msgstr "Změněný uzel {name_value} na následující nastavení:\n"
"Hostitel: {host_value}\n"
"Přístav: {port_value}\n"
"Heslo: {password_value}\n"
"Časový limit obnovení: {resume_timeout_value}\n"
"Pouze vyhledávání: {search_only_value}\n"
"SSL: {ssl_value}\n"
"Zakázané zdroje: {disabled_sources_value}\n"

