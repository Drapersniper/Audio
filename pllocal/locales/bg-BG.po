#: pllocal/cog.py:48
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Възпроизвеждане на локални файлове и папки от конфигурираното от собственика местоположение"

#: pllocal/cog.py:113
#, docstring
msgid "Configure cog settings"
msgstr "Конфигуриране на настройките за зъбно колело"

#: pllocal/cog.py:117
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Показване на версията на зъбното колело и неговите зависимости от PyLav"

#: pllocal/cog.py:133
msgid "Library / Cog"
msgstr "Библиотека / Зъбно колело"

#: pllocal/cog.py:134
msgid "Version"
msgstr "Версия"

#: pllocal/cog.py:148
#, docstring
msgid "Update the track list for /local"
msgstr "Актуализиране на списъка с песни за /local"

#: pllocal/cog.py:158
msgid "Local track list updated {number_value} currently present"
msgstr "Актуализиран е локалният списък с песни {number_value} В момента присъства"

#: pllocal/cog.py:185
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Възпроизвеждане на локален файл или папка, поддържа частично търсене"

#: pllocal/cog.py:193
msgid "{user_input_query_value} is not a valid local file or folder"
msgstr "{user_input_query_value} не е валиден локален файл или папка"

#: pllocal/cog.py:211
msgid "You must be in a voice channel to allow me to connect"
msgstr "Трябва да сте в гласов канал, за да ми позволите да се свържа"

#: pllocal/cog.py:224
msgid "I don't have permission to connect or speak in {channel_value}"
msgstr "Нямам разрешение да се свързвам или да говоря в {channel_value}"

#: pllocal/cog.py:249
msgid "{track_count_value} tracks enqueued."
msgstr "{track_count_value} Следите се редят на опашка."

#: pllocal/cog.py:257
msgid "{track_name_value} enqueued."
msgstr "{track_name_value} на опашката."

#: pllocal/cog.py:267
msgid "No tracks were found for your query."
msgstr "Не са намерени следи за вашата заявка."

