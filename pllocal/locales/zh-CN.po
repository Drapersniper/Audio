#: pllocal/cog.py:30
#, docstring
msgid "Play local files and folders from the owner configured location."
msgstr "从所有者配置的位置播放本地文件和文件夹。"

#: pllocal/cog.py:55
#, docstring
msgid "Configure cog settings."
msgstr "配置 cog 设置。"

#: pllocal/cog.py:59
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "显示Cog版本和它的 PyLav 依赖关系。"

#: pllocal/cog.py:72
msgid "Library/Cog"
msgstr "库 / COG"

#: pllocal/cog.py:72
msgid "Version"
msgstr "版本"

#: pllocal/cog.py:90
#, docstring
msgid "Play a local file or folder, supports partial searching."
msgstr "播放本地文件或文件夹，支持部分搜索。"

#: pllocal/cog.py:99
msgid "You need to be a DJ to play tracks."
msgstr "您需要是 DJ 才能播放曲目。"

#: pllocal/cog.py:117
msgid "You must be in a voice channel to allow me to connect."
msgstr "您必须在一个语音频道中才能允许我连接。"

#: pllocal/cog.py:130
msgid "I don't have permission to connect or speak in {channel}."
msgstr "我没有权限在 {channel} 中连接或说话。"

#: pllocal/cog.py:155
msgid "{track_count} tracks enqueued."
msgstr "{track_count} 曲目已排队."

#: pllocal/cog.py:163
msgid "{track} enqueued."
msgstr "{track} 已加入队列。"

#: pllocal/cog.py:173
msgid "No tracks were found for your query."
msgstr "没有找到您查询的曲目。"

