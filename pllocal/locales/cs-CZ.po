#: pllocal/cog.py:30
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Přehrát místní soubory a složky od vlastníka nakonfigurovaného umístění"

#: pllocal/cog.py:55
#, docstring
msgid "Configure cog settings"
msgstr "Konfigurace nastavení cog"

#: pllocal/cog.py:59
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies"
msgstr "Zobrazit verzi Cog a jsou to PyLav závislosti"

#: pllocal/cog.py:72
msgid "Library/Cog"
msgstr "Knihovna/Cog"

#: pllocal/cog.py:72
msgid "Version"
msgstr "Verze"

#: pllocal/cog.py:90
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Přehrát místní soubor nebo složku, podporuje částečné vyhledávání"

#: pllocal/cog.py:99
msgid "You need to be a DJ to play tracks"
msgstr "Musíš být DJ pro přehrávání skladeb"

#: pllocal/cog.py:117
msgid "You must be in a voice channel to allow me to connect"
msgstr "Musíš být v hlasovém kanálu, abych se mohl připojit"

#: pllocal/cog.py:130
msgid "I don't have permission to connect or speak in {channel}"
msgstr "Nemám oprávnění připojit se nebo mluvit v {channel}"

#: pllocal/cog.py:155
msgid "{track_count} tracks enqueued"
msgstr "{track_count} skladeb ve frontě"

#: pllocal/cog.py:163
msgid "{track} enqueued"
msgstr "{track} zařadil do fronty"

#: pllocal/cog.py:173
msgid "No tracks were found for your query"
msgstr "Pro váš dotaz nebyly nalezeny žádné skladby"

