#: pllocal/cog.py:30
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Toista paikalliset tiedostot ja kansiot omistajalta määritetty sijainti"

#: pllocal/cog.py:55
#, docstring
msgid "Configure cog settings"
msgstr "Määritä koodekin asetukset"

#: pllocal/cog.py:59
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies"
msgstr "Näytä versio Cog ja se on PyLav riippuvuudet"

#: pllocal/cog.py:72
msgid "Library/Cog"
msgstr "Kirjasto/Kokki"

#: pllocal/cog.py:72
msgid "Version"
msgstr "Versio"

#: pllocal/cog.py:90
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Toista paikallinen tiedosto tai kansio, tukee osittaista hakua"

#: pllocal/cog.py:99
msgid "You need to be a DJ to play tracks"
msgstr "Sinun täytyy olla DJ soittaaksesi kappaleita"

#: pllocal/cog.py:117
msgid "You must be in a voice channel to allow me to connect"
msgstr "Sinun täytyy olla äänikanavalla, jotta voin muodostaa yhteyden"

#: pllocal/cog.py:130
msgid "I don't have permission to connect or speak in {channel}"
msgstr "Minulla ei ole oikeuksia muodostaa yhteyttä tai puhua {channel}"

#: pllocal/cog.py:155
msgid "{track_count} tracks enqueued"
msgstr "{track_count} kappaletta jonossa"

#: pllocal/cog.py:163
msgid "{track} enqueued"
msgstr "{track} asetettu jonoon"

#: pllocal/cog.py:173
msgid "No tracks were found for your query"
msgstr "Etsintäsi varten ei löytynyt kappaleita"

