#: pllocal/cog.py:30
#, docstring
msgid "Play local files and folders from the owner configured location."
msgstr "Odtwarzaj pliki lokalne i foldery z skonfigurowanej lokalizacji właściciela."

#: pllocal/cog.py:55
#, docstring
msgid "Configure cog settings."
msgstr "Skonfiguruj ustawienia cog."

#: pllocal/cog.py:59
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Pokaż wersję Cog i jego zależności PyLav."

#: pllocal/cog.py:72
msgid "Library/Cog"
msgstr "Biblioteka/Cog"

#: pllocal/cog.py:72
msgid "Version"
msgstr "Wersja"

#: pllocal/cog.py:90
#, docstring
msgid "Play a local file or folder, supports partial searching."
msgstr "Odtwarzaj lokalny plik lub folder, obsługuje częściowe wyszukiwanie."

#: pllocal/cog.py:99
msgid "You need to be a DJ to play tracks."
msgstr "Musisz być DJ-em, aby grać w utwory."

#: pllocal/cog.py:117
msgid "You must be in a voice channel to allow me to connect."
msgstr "Musisz być w kanale głosowym, aby pozwolić mi się połączyć."

#: pllocal/cog.py:130
msgid "I don't have permission to connect or speak in {channel}."
msgstr "Nie mam uprawnień do łączenia się lub mówienia w {channel}."

#: pllocal/cog.py:155
msgid "{track_count} tracks enqueued."
msgstr "{track_count} utworów w kolejce."

#: pllocal/cog.py:163
msgid "{track} enqueued."
msgstr "{track} w kolejce."

#: pllocal/cog.py:173
msgid "No tracks were found for your query."
msgstr "Nie znaleziono utworów dla Twojego zapytania."

