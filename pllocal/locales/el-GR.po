#: pllocal/cog.py:48
#, docstring
msgid "Play local files and folders from the owner configured location"
msgstr "Αναπαραγωγή τοπικών αρχείων και φακέλων από τη ρυθμισμένη τοποθεσία του ιδιοκτήτη"

#: pllocal/cog.py:113
#, docstring
msgid "Configure cog settings"
msgstr "Ρύθμιση ρυθμίσεων cog"

#: pllocal/cog.py:117
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Εμφάνιση της έκδοσης του Cog και των εξαρτήσεών του PyLav"

#: pllocal/cog.py:133
msgid "Library / Cog"
msgstr "Βιβλιοθήκη / Γρανάζι"

#: pllocal/cog.py:134
msgid "Version"
msgstr "Εκδοχή"

#: pllocal/cog.py:148
#, docstring
msgid "Update the track list for /local"
msgstr "Ενημέρωση της λίστας κομματιών για /local"

#: pllocal/cog.py:158
msgid "Local track list updated {number_value} currently present"
msgstr "Ενημερώθηκε η τοπική λίστα κομματιών {number_value} τωρινή παρουσία"

#: pllocal/cog.py:185
#, docstring
msgid "Play a local file or folder, supports partial searching"
msgstr "Αναπαραγωγή τοπικού αρχείου ή φακέλου, υποστηρίζει μερική αναζήτηση"

#: pllocal/cog.py:193
msgid "{user_input_query_value} is not a valid local file or folder"
msgstr "{user_input_query_value} δεν είναι έγκυρο τοπικό αρχείο ή φάκελος"

#: pllocal/cog.py:211
msgid "You must be in a voice channel to allow me to connect"
msgstr "Πρέπει να βρίσκεστε σε ένα κανάλι φωνής για να μου επιτρέψετε να συνδεθώ"

#: pllocal/cog.py:224
msgid "I don't have permission to connect or speak in {channel_value}"
msgstr "Δεν έχω δικαίωμα σύνδεσης ή ομιλίας {channel_value}"

#: pllocal/cog.py:249
msgid "{track_count_value} tracks enqueued."
msgstr "{track_count_value} κομμάτια εγκιβωτισμένα."

#: pllocal/cog.py:257
msgid "{track_name_value} enqueued."
msgstr "{track_name_value} στην ουρά."

#: pllocal/cog.py:267
msgid "No tracks were found for your query."
msgstr "Δεν βρέθηκαν κομμάτια για το ερώτημά σας."

