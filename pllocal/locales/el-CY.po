#: pllocal/cog.py:30
#, docstring
msgid "Play local files and folders from the owner configured location."
msgstr "Αναπαραγωγή τοπικών αρχείων και φακέλων από την τοποθεσία που έχει ρυθμιστεί ο ιδιοκτήτης."

#: pllocal/cog.py:55
#, docstring
msgid "Configure cog settings."
msgstr "Ρύθμιση ρυθμίσεων κώδικα."

#: pllocal/cog.py:59
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Εμφάνιση της έκδοσης του Cog και είναι PyLav εξαρτήσεις."

#: pllocal/cog.py:72
msgid "Library/Cog"
msgstr "Βιβλιοθήκη/Σόργο"

#: pllocal/cog.py:72
msgid "Version"
msgstr "Έκδοση"

#: pllocal/cog.py:86
#, docstring
msgid "Play a local file or folder, supports partial searching."
msgstr "Αναπαραγωγή τοπικού αρχείου ή φακέλου, υποστηρίζει μερική αναζήτηση."

#: pllocal/cog.py:95
msgid "You need to be a DJ to play tracks."
msgstr "Θα πρέπει να είναι ένα DJ για να παίξει κομμάτια."

#: pllocal/cog.py:113
msgid "You must be in a voice channel to allow me to connect."
msgstr "Πρέπει να είστε σε ένα φωνητικό κανάλι για να μου επιτρέψετε να συνδεθώ."

#: pllocal/cog.py:126
msgid "I don't have permission to connect or speak in {channel}."
msgstr "Δεν έχω άδεια να συνδεθώ ή να μιλήσω στο {channel}."

#: pllocal/cog.py:151
msgid "{track_count} tracks enqueued."
msgstr "{track_count} κομμάτια στην ουρά."

#: pllocal/cog.py:159
msgid "{track} enqueued."
msgstr "{track} στην ουρά."

#: pllocal/cog.py:169
msgid "No tracks were found for your query."
msgstr "Δεν βρέθηκαν κομμάτια για το ερώτημά σας."

