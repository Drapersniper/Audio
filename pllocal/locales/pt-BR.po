#: pllocal/cog.py:30
#, docstring
msgid "Play local files and folders from the owner configured location."
msgstr "Reproduzir arquivos e pastas locais do local configurado pelo proprietário."

#: pllocal/cog.py:55
#, docstring
msgid "Configure cog settings."
msgstr "Configurar configurações de cog."

#: pllocal/cog.py:59
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Mostra a versão do Cog e suas dependências PyLav."

#: pllocal/cog.py:72
msgid "Library/Cog"
msgstr "Biblioteca/Cog"

#: pllocal/cog.py:72
msgid "Version"
msgstr "Versão"

#: pllocal/cog.py:86
#, docstring
msgid "Play a local file or folder, supports partial searching."
msgstr "Reproduzir um arquivo local ou pasta, suporta busca parcial."

