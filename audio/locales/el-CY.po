#: audio/hybrid_commands.py:384
msgid "{queue_size} tracks shuffled"
msgstr "{queue_size} κομμάτια ανακατεύονται"

#: audio/hybrid_commands.py:410
msgid "Repeating the queue"
msgstr "Επανάληψη της ουράς"

#: audio/hybrid_commands.py:413
msgid "Repeating disabled"
msgstr "Επανάληψη απενεργοποιημένη"

#: audio/hybrid_commands.py:416
