msgstr "Ο παίκτης ήδη σε παύση εννοούσε να τρέξει το `{prefix}{command}`."

#: audio/hybrid_commands.py:455
msgid "Player paused."
msgstr "Παίκτης σε παύση."

#: audio/hybrid_commands.py:476
msgid "Player already paused did you mean to run `/pause`."
msgstr "Ο παίκτης ήδη σε παύση εννοείτε να τρέξει το `/pause`."

#: audio/hybrid_commands.py:489
msgid "Player resumed."
msgstr "Ο παίκτης συνεχίστηκε."

#: audio/hybrid_commands.py:519
msgid "Volume cannot be higher than {max_volume}."
msgstr "Ο τόμος δεν μπορεί να είναι μεγαλύτερος από {max_volume}."

#: audio/hybrid_commands.py:528 audio/utility_commands.py:61
msgid "Player volume set to {volume}%."
msgstr "Η ένταση του παίκτη ορίστηκε σε {volume}%."

#: audio/hybrid_commands.py:560
msgid "Nothing playing."
msgstr "Τίποτα δεν παίζει."

#: audio/hybrid_commands.py:569
msgid "Unable to seek track"
msgstr "Δεν είναι δυνατή η αναζήτηση ίχνους"

#: audio/hybrid_commands.py:570
msgid "Can't seek on a stream."
msgstr "Δεν είναι δυνατή η αναζήτηση σε ένα ρεύμα."

#: audio/hybrid_commands.py:578
msgid "Unable to seek track."
msgstr "Δεν είναι δυνατή η αναζήτηση κομματιού."

#: audio/hybrid_commands.py:587
