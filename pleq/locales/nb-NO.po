#: pleq/cog.py:30
#, docstring
msgid "Apply equalizer presets to the PyLav player"
msgstr "Bruk equalizer forhåndsinstillinger på PyLav spiller"

#: pleq/cog.py:36
msgid "Apply an Equalizer preset to the player"
msgstr "Påfør en Equalizer forhåndsinnstilling til spilleren"

#: pleq/cog.py:50
#, docstring
msgid "Configure the Player behaviour when a preset is set"
msgstr "Konfigurer spilleroppførselen når en forhåndsinnstilling er satt"

#: pleq/cog.py:54
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies"
msgstr "Vis versjonen av Cog dens PyLav avhengigheter"

#: pleq/cog.py:70
msgid "Library/Cog"
msgstr "Bibliotek/Hund"

#: pleq/cog.py:71
msgid "Version"
msgstr "Versjon"

#: pleq/cog.py:84
#, docstring
msgid "Persist the last used preset"
msgstr "Vedvare den sist brukte forhåndsinnstillingen"

#: pleq/cog.py:100
msgid "The player will now apply the last used preset when it is created"
msgstr "Spilleren vil nå bruke den sist brukte forhåndsinnstillingen når den er opprettet"

#: pleq/cog.py:112
msgid "Last used preset will no longer be saved"
msgstr "Sist brukte forhåndsinnstillinger vil ikke bli lagret"

#: pleq/cog.py:123
#, docstring
msgid "Apply a Bass boost preset to the player.\n\n"
"        Arguments:\n"
"            - Maximum\n"
"            - Insane\n"
"            - Extreme\n"
"            - High\n"
"            - Very High\n"
"            - Medium\n"
"            - Fined Tuned\n"
"            - Cut-off\n"
"            - Off\n"
"        "
msgstr "Påfør en Bassboost forhåndsinnstilling til spilleren.\n\n"
"        Argumenter:\n"
"            - Maksimum\n"
"            - Insane\n"
"            - Ekstrem\n"
"            - Høy\n"
"            - Svært høyt\n"
"            - Medium\n"
"            - Finert slått oppe\n"
"            - Cut-off\n"
"            - Off\n"
"        "

