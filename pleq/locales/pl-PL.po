#: pleq/cog.py:24
#, docstring
msgid "Apply equalizer presets to the PyLav player."
msgstr "Zastosuj ustawienia korektora do odtwarzacza PyLav."

#: pleq/cog.py:51
#, docstring
msgid "Configure the Player behaviour when a preset is set."
msgstr "Skonfiguruj zachowanie gracza, gdy ustawienie jest ustawione."

#: pleq/cog.py:55
#, docstring
msgid "Persist the last used preset."
msgstr "Zachowaj ostatnio używane ustawienia."

#: pleq/cog.py:70
msgid "The player will now apply the last used preset when it is created."
msgstr "Gracz zastosuje ostatnio używane ustawienia po utworzeniu."

#: pleq/cog.py:81
msgid "Last used preset will no longer be saved."
msgstr "Ostatnio używane ustawienia nie będą już zapisywane."

#: pleq/cog.py:95
#, docstring
msgid "Apply a Bass boost preset to the player.\n\n"
"        Arguments:\n"
"            - Maximum\n"
"            - Insane\n"
"            - Extreme\n"
"            - High\n"
"            - Very High\n"
"            - Medium\n"
"            - Cut-off\n"
"            - Off\n"
"        "
msgstr "Zastosuj ustawienia podbicia basu do gracza.\n\n"
"        Arguments:\n"
"            - Maksymalnie\n"
"            - Insane\n"
"            - Ekstremalne\n"
"            - Wysokie\n"
"            - Bardzo Wysokie\n"
"            - Średnie\n"
"            - Wycięcie\n"
"            - Wyłączone\n"
"        "

#: pleq/cog.py:120
msgid "Bass boost preset has been disabled."
msgstr "Ustawienia podbicia basu zostały wyłączone."

#: pleq/cog.py:127
msgid "Bass boost - Maximum"
msgstr "Podbicie basu - Maksimum"

#: pleq/cog.py:131
msgid "Bass boost - Insane"
msgstr "Podbicie basu - Insane"

#: pleq/cog.py:135
msgid "Bass boost - Extreme"
msgstr "Podbicie basu - Ekstremalne"

#: pleq/cog.py:139
msgid "Bass boost - Very High"
msgstr "Podbicie basu - Bardzo wysoka"

#: pleq/cog.py:143
msgid "Bass boost - High"
msgstr "Podbicie basów - Wysokie"

#: pleq/cog.py:147
msgid "Bass boost - Medium"
msgstr "Podbicie basu - Średni"

#: pleq/cog.py:151
msgid "Bass boost - Cut-off"
msgstr "Podbicie basu - odcięcie"

#: pleq/cog.py:163
msgid "Preset has been set to {name}."
msgstr "Ustawienie zostało ustawione na {name}."

#: pleq/cog.py:170
#, docstring
msgid "Apply a Piano preset to the player.\n\n"
"        Suitable for acoustic tracks, or tacks with an emphasis on female vocals.\n\n"
"        Can also be used as a bass cut-off.\n"
"        "
msgstr "Zastosuj ustawienia Piano do gracza.\n\n"
"        Struktura danych sd_sd_sd_sd_sd_sd_sd_sd\n\n"
"        Może być również używany jako bas.\n"
"        "

#: pleq/cog.py:194
msgid "Piano"
msgstr "Pianina"

#: pleq/cog.py:207
msgid "Piano preset has been set."
msgstr "Ustawienia Piano zostały ustawione."

#: pleq/cog.py:214
#, docstring
msgid "Apply an experimental Metal/Rock equalizer preset.\n\n"
"        Expect clipping on songs with heavy bass.\n"
"        "
msgstr "Zastosuj eksperymentalne ustawienie korektora metalowego/skalistego.\n\n"
"        Oczekiwane przycinanie na piosenkach z ciężkimi bazy.\n"
"        "

#: pleq/cog.py:238
msgid "Metal"
msgstr "Metale"

#: pleq/cog.py:250
msgid "Rock/Metal preset has been set."
msgstr "Ustawienie Rock/Metal zostało ustawione."

#: pleq/cog.py:257
#, docstring
msgid "Remove any equalizer preset from the player."
msgstr "Usuń ustawienie korektora z gracza."

#: pleq/cog.py:270
msgid "Equalizer preset has been reset."
msgstr "Ustawienie korektora zostało zresetowane."

