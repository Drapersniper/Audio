#: plradio/cog.py:73
#, docstring
msgid "Enqueue a radio station"
msgstr "Zastavit do fronty rozhlasové stanice"

#: plradio/cog.py:81
msgid "Multiple stations matched, pick the one which you meant"
msgstr "Odpovídá více stanic, vyberte si ty, které jste chtěli"

#: plradio/cog.py:82
msgid "Pick a station"
msgstr "Vybrat stanici"

#: plradio/cog.py:94
msgid "You must be in a voice channel to allow me to connect"
msgstr "Musíš být v hlasovém kanálu, abych se mohl připojit"

#: plradio/cog.py:104
msgid "I don't have permission to connect or speak in {channel}"
msgstr "Nemám oprávnění připojit se nebo mluvit v {channel}"

#: plradio/cog.py:133
msgid "Enqueued"
msgstr "Zařazeno do fronty"

#: plradio/cog.py:145
msgid "Unable to play"
msgstr "Nelze přehrát"

