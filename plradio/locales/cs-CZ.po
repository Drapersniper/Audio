#: plradio/cog.py:73
#, docstring
msgid "Enqueue a radio station."
msgstr "Zastavte do fronty rozhlasové stanice."

#: plradio/cog.py:81
msgid "Multiple stations matched, pick the one which you meant."
msgstr "Vícenásobné stanice se shodují, vyberte si ty, které máte na mysli."

#: plradio/cog.py:82
msgid "Pick a station"
msgstr "Vybrat stanici"

#: plradio/cog.py:94
msgid "You must be in a voice channel to allow me to connect."
msgstr "Musíš být v hlasovém kanálu, abych se mohl připojit."

#: plradio/cog.py:104
msgid "I don't have permission to connect or speak in {channel}."
msgstr "Nemám oprávnění připojit se nebo mluvit v {channel}."

#: plradio/cog.py:133
msgid "Enqueued **[{station_name}]({station_url})**."
msgstr "Enqueued **[{station_name}]({station_url})**."

#: plradio/cog.py:145
msgid "Unable to play **[{station_name}]({station_url})**."
msgstr "Nelze přehrát **[{station_name}]({station_url})**."

