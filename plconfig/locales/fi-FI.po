#: plconfig/cog.py:28
#, docstring
msgid "Configure PyLav library settings."
msgstr "Määritä Pylav-kirjaston asetukset."

#: plconfig/cog.py:39
#, docstring
msgid "Change global settings for PyLav"
msgstr "Muuta yleisiä asetuksia PyLV:lle"

#: plconfig/cog.py:43
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Näytä versio Cog ja se on PyLav riippuvuudet."

#: plconfig/cog.py:56
msgid "Library/Cog"
msgstr "Kirjasto/Kokki"

#: plconfig/cog.py:56
msgid "Version"
msgstr "Versio"

#: plconfig/cog.py:64
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Aseta kansio PyLavin asetustiedostoille\n\n"
"        Muutokset otetaan käyttöön käynnistyksen jälkeen.\n"
"        "

#: plconfig/cog.py:76 plconfig/cog.py:130
msgid "{folder} is not a folder."
msgstr "{folder} ei ole kansio."

#: plconfig/cog.py:90 plconfig/cog.py:144
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} ei ole olemassa, suorita komento uudelleen luomalla parametri arvoksi 1 luodaksesi tämän kansion automaattisesti."

#: plconfig/cog.py:108
msgid "PyLav's config folder has been set to {folder}."
msgstr "PyLavin asetuskansio on asetettu arvoon {folder}."

#: plconfig/cog.py:118
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after uudelleenkäynnistämisen jälkeen bot.\n"
"        "

#: plconfig/cog.py:165
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "PyLavin paikalliset kappaleet on asetettu kansioon {folder}."

#: plconfig/cog.py:175
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after uudelleenkäynnistämisen jälkeen.\n"
"        "

#: plconfig/cog.py:192
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} ei ole olemassa, suorita komento uudelleen java-parametrilla on asetettu oikealle polulle."

#: plconfig/cog.py:208
msgid "{java} is not an executable file."
msgstr "{java} ei ole suoritettava tiedosto."

#: plconfig/cog.py:220
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} ei ole suoritettava, suorita komento uudelleen java-parametrilla on asetettu oikealle polulle."

#: plconfig/cog.py:238
msgid "PyLav's java executable has been set to {java}."
msgstr "PyLavin java-tiedosto on asetettu arvoon {java}."

#: plconfig/cog.py:248
#, docstring
msgid "Change the managed node configuration."
msgstr "Vaihda hallitun solmun asetuksia."

#: plconfig/cog.py:252
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Vaihda hallitun solmun päälle/pois.\n\n"
"        Muutokset otetaan käyttöön käynnistyksen jälkeen.\n"
"        "

#: plconfig/cog.py:267
msgid "PyLav's managed node has been enabled."
msgstr "PyLavin hallinnoitu solmu on otettu käyttöön."

#: plconfig/cog.py:275
msgid "PyLav's managed node has been disabled."
msgstr "PyLavin hallinnoitu solmu on poistettu käytöstä."

#: plconfig/cog.py:283
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Vaihda hallitun palvelimen automaattiset päivitykset päälle/pois.\n\n"
"        Muutokset otetaan käyttöön käynnistyksen jälkeen.\n"
"        "

#: plconfig/cog.py:298
msgid "PyLav's managed node auto updates have been enabled."
msgstr "PyLavin hallinnoidut solmun automaattiset päivitykset on otettu käyttöön."

#: plconfig/cog.py:306
msgid "PyLav's managed node auto updates have been disabled."
msgstr "PyLavin hallinnoidut palvelimen automaattiset päivitykset on poistettu käytöstä."

