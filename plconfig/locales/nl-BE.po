#: plconfig/cog.py:29
#, docstring
msgid "Configure PyLav library settings."
msgstr "PyLav bibliotheek instellingen configureren."

#: plconfig/cog.py:42
#, docstring
msgid "Change global settings for PyLav"
msgstr "Verander algemene instellingen voor PyLav"

#: plconfig/cog.py:46
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Toon de versie van de Cog en de PyLav afhankelijkheden."

#: plconfig/cog.py:59
msgid "Library/Cog"
msgstr "Bibliotheek/Cog"

#: plconfig/cog.py:59
msgid "Version"
msgstr "Versie"

#: plconfig/cog.py:67
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Stel de map in voor PyLav's configuratiebestanden\n\n"
"        Wijzigingen worden toegepast na het herstarten van de bot.\n"
"        "

#: plconfig/cog.py:79 plconfig/cog.py:133
msgid "{folder} is not a folder."
msgstr "{folder} is geen map."

#: plconfig/cog.py:93 plconfig/cog.py:147
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} bestaat niet, voer het commando opnieuw uit met het aanmaak argument op 1 om deze map automatisch te maken."

#: plconfig/cog.py:111
msgid "PyLav's config folder has been set to {folder}."
msgstr "PyLav's configuratiemap is ingesteld op {folder}."

#: plconfig/cog.py:121
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Stel de lokale track-map in voor PyLav.\n\n"
"        Wijzigingen zullen worden toegepast na het herstarten van de bot.\n"
"        "

#: plconfig/cog.py:168
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "PyLav's lokale track-map is ingesteld op {folder}."

#: plconfig/cog.py:178
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Stel het uitvoerbare java in voor PyLav.\n\n"
"        Standaard is \"java\"\n"
"        Wijzigingen zullen worden toegepast na het herstarten van de bot.\n"
"        "

#: plconfig/cog.py:195
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} bestaat niet, voer de opdracht opnieuw uit met het argument van de java op het juiste pad."

#: plconfig/cog.py:211
msgid "{java} is not an executable file."
msgstr "{java} is geen uitvoerbaar bestand."

#: plconfig/cog.py:223
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} is geen uitvoerbaar, voer de opdracht opnieuw uit met het argument dat is ingesteld op het juiste pad."

#: plconfig/cog.py:241
msgid "PyLav's java executable has been set to {java}."
msgstr "PyLav's java uitvoerbaar is ingesteld op {java}."

#: plconfig/cog.py:251
#, docstring
msgid "Change the managed node configuration."
msgstr "Wijzig de beheerde node configuratie."

#: plconfig/cog.py:255
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Zet de beheerde node aan/uit.\n\n"
"        Wijzigingen worden toegepast na het herstarten van de bot.\n"
"        "

#: plconfig/cog.py:270
msgid "PyLav's managed node has been enabled."
msgstr "PyLav's beheerde node is ingeschakeld."

#: plconfig/cog.py:278
msgid "PyLav's managed node has been disabled."
msgstr "PyLav's beheerde node is uitgeschakeld."

#: plconfig/cog.py:286
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Schakel de beheerde node automatisch updates aan/uit.\n\n"
"        Wijzigingen worden toegepast na het herstarten van de bot.\n"
"        "

#: plconfig/cog.py:301
msgid "PyLav's managed node auto updates have been enabled."
msgstr "PyLav's beheerde automatische node updates zijn ingeschakeld."

#: plconfig/cog.py:309
msgid "PyLav's managed node auto updates have been disabled."
msgstr "PyLav's beheerde automatische node updates zijn uitgeschakeld."

#: plconfig/cog.py:317
#, docstring
msgid "Toggle the managed external node on/off."
msgstr "Zet de beheerde externe node aan/uit."

#: plconfig/cog.py:335
msgid "PyLav's managed external node has been enabled."
msgstr "PyLav's beheerde externe node is ingeschakeld."

#: plconfig/cog.py:343
msgid "PyLav's managed external node has been disabled."
msgstr "PyLav's beheerde externe node is uitgeschakeld."

