#: plconfig/cog.py:24
#, docstring
msgid "Configure PyLav library settings."
msgstr "Ρύθμιση παραμέτρων βιβλιοθήκης PyLav."

#: plconfig/cog.py:35
#, docstring
msgid "Change global settings for PyLav"
msgstr "Αλλαγή καθολικών ρυθμίσεων για το PyLav"

#: plconfig/cog.py:39
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ορίστε το φάκελο για τα αρχεία ρυθμίσεων PyLav's\n\n"
"        Οι αλλαγές θα εφαρμοστούν μετά την επανεκκίνηση του bot.\n"
"        "

#: plconfig/cog.py:51 plconfig/cog.py:105
msgid "{folder} is not a folder."
msgstr "{folder} δεν είναι φάκελος."

#: plconfig/cog.py:65 plconfig/cog.py:119
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} δεν υπάρχει, εκτελέστε ξανά την εντολή με το όρισμα δημιουργίας που ορίζεται στο 1 για να δημιουργήσετε αυτόματα αυτόν το φάκελο."

#: plconfig/cog.py:83
msgid "PyLav's config folder has been set to {folder}."
msgstr "Ο φάκελος ρυθμίσεων της PyLav's έχει οριστεί σε {folder}."

#: plconfig/cog.py:93
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ορίστε τον τοπικό φάκελο κομματιών για το PyLav.\n\n"
"        Οι αλλαγές θα εφαρμοστούν μετά την επανεκκίνηση του bot.\n"
"        "

#: plconfig/cog.py:140
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "Ο φάκελος τοπικών κομματιών της PyLav's έχει οριστεί σε {folder}."

#: plconfig/cog.py:150
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ορίστε το εκτελέσιμο java για το PyLav.\n\n"
"        Η προεπιλογή είναι \"java\"\n"
"        Οι αλλαγές θα εφαρμοστούν μετά την επανεκκίνηση του bot.\n"
"        "

#: plconfig/cog.py:166
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} δεν υπάρχει, εκτελέστε ξανά την εντολή με το όρισμα java που έχει οριστεί στη σωστή διαδρομή."

#: plconfig/cog.py:182
msgid "{java} is not an executable file."
msgstr "{java} δεν είναι εκτελέσιμο αρχείο."

#: plconfig/cog.py:194
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} δεν είναι εκτελέσιμο, εκτελέστε την εντολή ξανά με το όρισμα java να έχει οριστεί στη σωστή διαδρομή."

#: plconfig/cog.py:212
msgid "PyLav's java executable has been set to {java}."
msgstr "Το εκτελέσιμο java της PyLava έχει οριστεί σε {java}."

#: plconfig/cog.py:222
#, docstring
msgid "Change the managed node configuration."
msgstr "Αλλάξτε τις διαχειριζόμενες ρυθμίσεις κόμβου."

#: plconfig/cog.py:226
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Εναλλαγή του διαχειριζόμενου κόμβου on/off.\n\n"
"        Οι αλλαγές θα εφαρμοστούν μετά την επανεκκίνηση του bot.\n"
"        "

#: plconfig/cog.py:241
msgid "PyLav's managed node has been enabled."
msgstr "Ο διαχειριζόμενος κόμβος της PyLav έχει ενεργοποιηθεί."

#: plconfig/cog.py:249
msgid "PyLav's managed node has been disabled."
msgstr "Ο διαχειριζόμενος κόμβος της PyLav έχει απενεργοποιηθεί."

#: plconfig/cog.py:257
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ενεργοποίηση/απενεργοποίηση αυτόματων ενημερώσεων κόμβου.\n\n"
"        Οι αλλαγές θα εφαρμοστούν μετά την επανεκκίνηση του bot.\n"
"        "

#: plconfig/cog.py:272
msgid "PyLav's managed node auto updates have been enabled."
msgstr "Οι διαχειριζόμενες αυτόματες ενημερώσεις κόμβων PyLav's έχουν ενεργοποιηθεί."

#: plconfig/cog.py:280
msgid "PyLav's managed node auto updates have been disabled."
msgstr "Οι διαχειριζόμενες αυτόματες ενημερώσεις κόμβων της PyLav's έχουν απενεργοποιηθεί."

