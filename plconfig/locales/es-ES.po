#: plconfig/cog.py:43
#, docstring
msgid "Change global settings for PyLav"
msgstr "Cambiar la configuración global de PyLav"

#: plconfig/cog.py:47
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Establezca la carpeta para los archivos de configuración de PyLav\n\n"
"        Los cambios se aplicarán después de reiniciar el bot.\n"
"        "

#: plconfig/cog.py:59 plconfig/cog.py:113
msgid "{folder} is not a folder."
msgstr "{folder} no es una carpeta."

#: plconfig/cog.py:73 plconfig/cog.py:127
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} no existe, vuelva a ejecutar el comando con el argumento create set a 1 para crear automáticamente esta carpeta."

#: plconfig/cog.py:91
msgid "PyLav's config folder has been set to {folder}."
msgstr "La carpeta de configuración de PyLavel se ha establecido en {folder}."

#: plconfig/cog.py:101
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Establezca la carpeta de pistas locales para PyLav.\n\n"
"        Los cambios se aplicarán después de reiniciar el bot.\n"
"        "

#: plconfig/cog.py:149
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "La carpeta de pistas locales de PyLave se ha establecido en {folder}."

#: plconfig/cog.py:159
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Establece el ejecutable de java para PyLav.\n\n"
"        Por defecto es \"java\"\n"
"        Se aplicarán los cambios después de reiniciar el bot.\n"
"        "

#: plconfig/cog.py:175
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} no existe, ejecuta de nuevo el comando con el argumento java establecido en la ruta correcta."

#: plconfig/cog.py:191
msgid "{java} is not an executable file."
msgstr "{java} no es un archivo ejecutable"

#: plconfig/cog.py:203
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} no es un ejecutable, ejecuta el comando de nuevo con el argumento java establecido en la ruta correcta."

#: plconfig/cog.py:221
msgid "PyLav's java executable has been set to {java}."
msgstr "El ejecutable de java de PyLave se ha establecido en {java}."

#: plconfig/cog.py:231
#, docstring
msgid "Change the managed node configuration."
msgstr "Cambiar la configuración del nodo administrado."

#: plconfig/cog.py:235
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Encender/apagar el nodo administrado.\n\n"
"        Se aplicarán los cambios después de reiniciar el bot.\n"
"        "

#: plconfig/cog.py:250
msgid "PyLav's managed node has been enabled."
msgstr "Se ha habilitado el nodo administrado de PyLav."

#: plconfig/cog.py:258
msgid "PyLav's managed node has been disabled."
msgstr "Se ha desactivado el nodo administrado de PyLav."

#: plconfig/cog.py:266
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Encender/apagar las actualizaciones automáticas del nodo administrado.\n\n"
"        Se aplicarán los cambios después de reiniciar el bot.\n"
"        "

#: plconfig/cog.py:281
msgid "PyLav's managed node auto updates have been enabled."
msgstr "Se han habilitado las actualizaciones automáticas de nodos administrados por PyLav."

#: plconfig/cog.py:289
msgid "PyLav's managed node auto updates have been disabled."
msgstr "Las actualizaciones automáticas de nodo administradas por PyLave han sido deshabilitadas."

