#: plconfig/cog.py:29
#, docstring
msgid "Configure PyLav library settings."
msgstr "Konfigurer PyLav bibliotek innstillinger."

#: plconfig/cog.py:42
#, docstring
msgid "Change global settings for PyLav"
msgstr "Endre globale innstillinger for PyLav"

#: plconfig/cog.py:46
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Vis versjonen av Cog dens PyLav avhengigheter."

#: plconfig/cog.py:59
msgid "Library/Cog"
msgstr "Bibliotek/Hund"

#: plconfig/cog.py:59
msgid "Version"
msgstr "Versjon"

#: plconfig/cog.py:67
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Angi mappen for PyLav's konfigurasjonsfiler\n\n"
"        Endringer vil bli brukt etter omstart av boten.\n"
"        "

#: plconfig/cog.py:79 plconfig/cog.py:133
msgid "{folder} is not a folder."
msgstr "{folder} er ikke en mappe."

#: plconfig/cog.py:93 plconfig/cog.py:147
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} finnes ikke, kjør kommandoen igjen med opprettelsesargumentet satt til 1 for å automatisk opprette denne mappen."

#: plconfig/cog.py:111
msgid "PyLav's config folder has been set to {folder}."
msgstr "PyLav's konfigurasjonsmappe er satt til {folder}."

#: plconfig/cog.py:121
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Angi de lokale sporene mappen for PyLav.\n\n"
"        Endringer vil bli brukt etter at boten har startet på nytt.\n"
"        "

#: plconfig/cog.py:168
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "PyLav's lokale mappe har blitt satt til {folder}."

#: plconfig/cog.py:178
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Angi java kjørbar for PyLav.\n\n"
"        Standard er \"java\"\n"
"        Endringer vil bli brukt etter at du har startet botten på nytt.\n"
"        "

#: plconfig/cog.py:195
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} finnes ikke, kjør kommandoen igjen med java argument satt til korrekt bane."

#: plconfig/cog.py:211
msgid "{java} is not an executable file."
msgstr "{java} er ikke en kjørbar fil."

#: plconfig/cog.py:223
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} er ikke en kjørbar, kjør kommandoen igjen med javaargumentet satt til korrekt sti."

#: plconfig/cog.py:241
msgid "PyLav's java executable has been set to {java}."
msgstr "PyLav's java kjørbare er satt til {java}."

#: plconfig/cog.py:251
#, docstring
msgid "Change the managed node configuration."
msgstr "Endre den administrerte node-konfigurasjonen."

#: plconfig/cog.py:255
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Slå av/på behandlet node på/av.\n\n"
"        Endringer vil bli brukt etter at du har startet boten på nytt.\n"
"        "

#: plconfig/cog.py:270
msgid "PyLav's managed node has been enabled."
msgstr "PyLav's administrerte node har blitt aktivert."

#: plconfig/cog.py:278
msgid "PyLav's managed node has been disabled."
msgstr "PyLav's administrerte node er deaktivert."

#: plconfig/cog.py:286
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Slå av/på den administrerte node automatisk oppdateringer på/av.\n\n"
"        Endringer vil bli brukt etter omstart av boten.\n"
"        "

#: plconfig/cog.py:301
msgid "PyLav's managed node auto updates have been enabled."
msgstr "PyLav's administrerte node automatisk oppdateringer har blitt aktivert."

#: plconfig/cog.py:309
msgid "PyLav's managed node auto updates have been disabled."
msgstr "PyLav's administrerte node oppdateringer har blitt deaktivert."

#: plconfig/cog.py:317
#, docstring
msgid "Toggle the managed external node on/off."
msgstr "Slå av/på den administrerte eksterne EK-gruppen."

#: plconfig/cog.py:335
msgid "PyLav's managed external node has been enabled."
msgstr "PyLav's administrerte ekstern node er aktivert."

#: plconfig/cog.py:343
msgid "PyLav's managed external node has been disabled."
msgstr "PyLav's administrerte ekstern node er deaktivert."

