#: plconfig/cog.py:43
#, docstring
msgid "Change global settings for PyLav"
msgstr "Změnit globální nastavení pro PyLav"

#: plconfig/cog.py:47
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Nastavte složku pro PyLav's config files\n\n"
"        Změny budou aplikovány po restartování botu.\n"
"        "

#: plconfig/cog.py:59 plconfig/cog.py:113
msgid "{folder} is not a folder."
msgstr "{folder} není složka."

#: plconfig/cog.py:73 plconfig/cog.py:127
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} neexistuje, spusťte příkaz znovu s vytvořeným argumentem nastaveným na 1 pro automatické vytvoření této složky."

#: plconfig/cog.py:91
msgid "PyLav's config folder has been set to {folder}."
msgstr "PyLavova konfigurační složka byla nastavena na {folder}."

#: plconfig/cog.py:101
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Nastavte místní složku pro PyLav.\n\n"
"        Změny budou aplikovány po restartu bota.\n"
"        "

#: plconfig/cog.py:149
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "Místní složka PyLavu byla nastavena na {folder}."

#: plconfig/cog.py:159
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Nastavte spustitelný soubor pro PyLav.\n\n"
"        Výchozí hodnota je \"java\"\n"
"        Změny budou aplikovány po restartování bota.\n"
"        "

#: plconfig/cog.py:175
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} neexistuje, spusťte příkaz znovu s argumentem java nastaveným na správnou cestu."

#: plconfig/cog.py:191
msgid "{java} is not an executable file."
msgstr "{java} není spustitelný soubor."

#: plconfig/cog.py:203
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} není spustitelný soubor, spusťte příkaz znovu s argumentem java nastaveným na správnou cestu."

#: plconfig/cog.py:221
msgid "PyLav's java executable has been set to {java}."
msgstr "Spustitelný soubor PyLava byl nastaven na {java}."

#: plconfig/cog.py:231
#, docstring
msgid "Change the managed node configuration."
msgstr "Změnit konfiguraci spravovaného uzlu."

#: plconfig/cog.py:235
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Zapnout/vypnout spravovaný uzel.\n\n"
"        Změny budou aplikovány po restartu bota.\n"
"        "

#: plconfig/cog.py:250
msgid "PyLav's managed node has been enabled."
msgstr "PyLav's spravovaný uzel byl povolen."

#: plconfig/cog.py:258
msgid "PyLav's managed node has been disabled."
msgstr "PyLav's spravovaný uzel byl zakázán."

#: plconfig/cog.py:266
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Zapnout/vypnout spravované aktualizace uzlu.\n\n"
"        Změny budou aplikovány po restartování botu.\n"
"        "

#: plconfig/cog.py:281
msgid "PyLav's managed node auto updates have been enabled."
msgstr "Automaticky spravované PyLav's Node aktualizace byly povoleny."

#: plconfig/cog.py:289
msgid "PyLav's managed node auto updates have been disabled."
msgstr "Automatické aktualizace PyLav's spravovaných Node byly zakázány."

