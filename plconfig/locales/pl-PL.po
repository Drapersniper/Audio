#: plconfig/cog.py:69
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ustaw folder dla plików konfiguracyjnych PyLava\n\n"
"        Zmiany zostaną zastosowane po ponownym uruchomieniu bota.\n"
"        "

#: plconfig/cog.py:81 plconfig/cog.py:135
msgid "{folder} is not a folder."
msgstr "{folder} nie jest katalogiem."

#: plconfig/cog.py:95 plconfig/cog.py:149
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} nie istnieje, uruchom ponownie polecenie z argumentem utworzenia ustawionym na 1, aby automatycznie utworzyć ten folder."

#: plconfig/cog.py:113
msgid "PyLav's config folder has been set to {folder}."
msgstr "Katalog konfiguracji PyLava został ustawiony na {folder}."

#: plconfig/cog.py:123
