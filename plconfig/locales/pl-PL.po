#: plconfig/cog.py:44
#, docstring
msgid "Change global settings for PyLav"
msgstr "Zmień ustawienia globalne dla PyLav"

#: plconfig/cog.py:48
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ustaw folder dla plików konfiguracyjnych PyLava\n\n"
"        Zmiany zostaną zastosowane po ponownym uruchomieniu bota.\n"
"        "

#: plconfig/cog.py:60 plconfig/cog.py:114
msgid "{folder} is not a folder."
msgstr "{folder} nie jest katalogiem."

#: plconfig/cog.py:74 plconfig/cog.py:128
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} nie istnieje, uruchom ponownie polecenie z argumentem utworzenia ustawionym na 1, aby automatycznie utworzyć ten folder."

#: plconfig/cog.py:92
msgid "PyLav's config folder has been set to {folder}."
msgstr "Katalog konfiguracji PyLava został ustawiony na {folder}."

#: plconfig/cog.py:102
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ustaw folder ścieżek lokalnych dla PyLav.\n\n"
"        Zmiany zostaną zastosowane po ponownym uruchomieniu bota.\n"
"        "

#: plconfig/cog.py:149
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "Lokalny folder ścieżek PyLava został ustawiony na {folder}."

#: plconfig/cog.py:159
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ustaw plik wykonywalny java dla PyLav.\n\n"
"        Domyślnie jest \"java\"\n"
"        Zmiany zostaną zastosowane po ponownym uruchomieniu bota.\n"
"        "

#: plconfig/cog.py:175
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} nie istnieje, uruchom ponownie polecenie z argumentem java ustawionym na właściwą ścieżkę."

#: plconfig/cog.py:191
msgid "{java} is not an executable file."
msgstr "{java} nie jest plikiem wykonywalnym."

#: plconfig/cog.py:203
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} nie jest wykonywalny, uruchom ponownie polecenie z argumentem java ustawionym na właściwą ścieżkę."

#: plconfig/cog.py:221
msgid "PyLav's java executable has been set to {java}."
msgstr "Plik wykonywalny PyLava został ustawiony na {java}."

#: plconfig/cog.py:231
#, docstring
msgid "Change the managed node configuration."
msgstr "Zmień konfigurację zarządzanego węzła."

#: plconfig/cog.py:235
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Włącz/wyłącz zarządzany węzeł.\n\n"
"        Zmiany zostaną zastosowane po ponownym uruchomieniu bota.\n"
"        "

#: plconfig/cog.py:250
msgid "PyLav's managed node has been enabled."
msgstr "Węzeł zarządzany przez PyLava został włączony."

#: plconfig/cog.py:258
msgid "PyLav's managed node has been disabled."
msgstr "Węzeł zarządzany przez PyLava został wyłączony."

#: plconfig/cog.py:266
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Włącz/wyłącz automatyczne aktualizacje zarządzanego węzła.\n\n"
"        Zmiany zostaną zastosowane po ponownym uruchomieniu bota.\n"
"        "

#: plconfig/cog.py:281
msgid "PyLav's managed node auto updates have been enabled."
msgstr "Automatyczna aktualizacja węzła zarządzanego przez PyLava została włączona."

#: plconfig/cog.py:289
msgid "PyLav's managed node auto updates have been disabled."
msgstr "Automatyczne aktualizacje węzła zarządzanego przez PyLava zostały wyłączone."

