#: plconfig/cog.py:45
#, docstring
msgid "Change global settings for PyLav"
msgstr "Ändra globala inställningar för PyLav"

#: plconfig/cog.py:49
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after start the bot.\n"
"        "

#: plconfig/cog.py:61 plconfig/cog.py:115
msgid "{folder} is not a folder."
msgstr "{folder} är inte en mapp."

#: plconfig/cog.py:75 plconfig/cog.py:129
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} finns inte, kör kommandot igen med skaparargumentet satt till 1 för att automatiskt skapa denna mapp."

#: plconfig/cog.py:93
msgid "PyLav's config folder has been set to {folder}."
msgstr "PyLavs konfigurationsmapp har satts till {folder}."

#: plconfig/cog.py:103
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Ställ in den lokala spårmappen för PyLav.\n\n"
"        Ändringar kommer att tillämpas efter att boten startats om.\n"
"        "

#: plconfig/cog.py:151
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "PyLavs lokala spårmapp har satts till {folder}."

#: plconfig/cog.py:161
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Set the java executable for PyLav.\n\n"
"        Standard är \"java\"\n"
"        Ändringar kommer att tillämpas efter omstart av boten.\n"
"        "

#: plconfig/cog.py:177
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} finns inte, kör kommandot igen med java-argumentet inställt på rätt sökväg."

#: plconfig/cog.py:193
msgid "{java} is not an executable file."
msgstr "{java} är inte en körbar fil."

#: plconfig/cog.py:205
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} är inte en körbar, kör kommandot igen med java-argumentet inställt på rätt sökväg."

#: plconfig/cog.py:223
msgid "PyLav's java executable has been set to {java}."
msgstr "PyLavs java körbar har satts till {java}."

#: plconfig/cog.py:233
#, docstring
msgid "Change the managed node configuration."
msgstr "Ändra den hanterade noden konfigurationen."

#: plconfig/cog.py:237
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Växla den hanterade noden på/av.\n\n"
"        Ändringar kommer att tillämpas efter omstart av boten.\n"
"        "

#: plconfig/cog.py:252
msgid "PyLav's managed node has been enabled."
msgstr "PyLavs hanterade nod har aktiverats."

#: plconfig/cog.py:260
msgid "PyLav's managed node has been disabled."
msgstr "PyLavs hanterade nod har inaktiverats."

#: plconfig/cog.py:268
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Växla den hanterade noden automatisk uppdateringar på/av.\n\n"
"        Ändringar kommer att tillämpas efter omstart av boten.\n"
"        "

#: plconfig/cog.py:283
msgid "PyLav's managed node auto updates have been enabled."
msgstr "PyLavs hanterade automatiska noduppdateringar har aktiverats."

#: plconfig/cog.py:291
msgid "PyLav's managed node auto updates have been disabled."
msgstr "PyLavs hanterade noduppdateringar har inaktiverats."

