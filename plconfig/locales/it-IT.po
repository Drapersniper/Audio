#: plconfig/cog.py:29
#, docstring
msgid "Configure PyLav library settings."
msgstr "Configura le impostazioni della libreria PyLav."

#: plconfig/cog.py:42
#, docstring
msgid "Change global settings for PyLav"
msgstr "Modifica le impostazioni globali per PyLav"

#: plconfig/cog.py:46
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Mostra la versione del Cog ed è le dipendenze di PyLAV."

#: plconfig/cog.py:59
msgid "Library/Cog"
msgstr "Library/Cog"

#: plconfig/cog.py:59
msgid "Version"
msgstr "Versione"

#: plconfig/cog.py:67
#, docstring
msgid "Set the folder for PyLav's config files\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Imposta la cartella per i file di configurazione di PyLav\n\n"
"        Le modifiche verranno applicate dopo il riavvio del bot.\n"
"        "

#: plconfig/cog.py:79 plconfig/cog.py:133
msgid "{folder} is not a folder."
msgstr "{folder} non è una cartella."

#: plconfig/cog.py:93 plconfig/cog.py:147
msgid "{folder} does not exist, run the command again with the create argument set to 1 to automatically create this folder."
msgstr "{folder} non esiste, esegui nuovamente il comando con l'argomento creato impostato a 1 per creare automaticamente questa cartella."

#: plconfig/cog.py:111
msgid "PyLav's config folder has been set to {folder}."
msgstr "La cartella di configurazione di PyLavè stata impostata a {folder}."

#: plconfig/cog.py:121
#, docstring
msgid "Set the local tracks folder for PyLav.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Imposta la cartella delle tracce locali per PyLav.\n\n"
"        Le modifiche verranno applicate dopo il riavvio del bot.\n"
"        "

#: plconfig/cog.py:168
msgid "PyLav's local tracks folder has been set to {folder}."
msgstr "La cartella delle tracce locali di PyLavè stata impostata a {folder}."

#: plconfig/cog.py:178
#, docstring
msgid "Set the java executable for PyLav.\n\n"
"        Default is \"java\"\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Imposta l'eseguibile java per PyLav.\n\n"
"        Il valore predefinito è \"java\"\n"
"        Le modifiche verranno applicate dopo il riavvio del bot.\n"
"        "

#: plconfig/cog.py:195
msgid "{java} does not exist, run the command again with the java argument set to the correct path."
msgstr "{java} non esiste, esegui nuovamente il comando con l'argomento java impostato sul percorso corretto."

#: plconfig/cog.py:211
msgid "{java} is not an executable file."
msgstr "{java} non è un file eseguibile."

#: plconfig/cog.py:223
msgid "{java} is not an executable, run the command again with the java argument set to the correct path."
msgstr "{java} non è un eseguibile, esegui nuovamente il comando con l'argomento java impostato sul percorso corretto."

#: plconfig/cog.py:241
msgid "PyLav's java executable has been set to {java}."
msgstr "L'eseguibile java di PyLavè stato impostato su {java}."

#: plconfig/cog.py:251
#, docstring
msgid "Change the managed node configuration."
msgstr "Cambia la configurazione del nodo gestito."

#: plconfig/cog.py:255
#, docstring
msgid "Toggle the managed node on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Attiva/disattiva il nodo gestito.\n\n"
"        Le modifiche verranno applicate dopo il riavvio del bot.\n"
"        "

#: plconfig/cog.py:270
msgid "PyLav's managed node has been enabled."
msgstr "Il nodo gestito di PyLavè stato abilitato."

#: plconfig/cog.py:278
msgid "PyLav's managed node has been disabled."
msgstr "Il nodo gestito di PyLavè stato disabilitato."

#: plconfig/cog.py:286
#, docstring
msgid "Toggle the managed node auto updates on/off.\n\n"
"        Changes will be applied after restarting the bot.\n"
"        "
msgstr "Attiva/disattiva gli aggiornamenti automatici del nodo gestito.\n\n"
"        Le modifiche verranno applicate dopo il riavvio del bot.\n"
"        "

#: plconfig/cog.py:301
msgid "PyLav's managed node auto updates have been enabled."
msgstr "Gli aggiornamenti automatici dei nodi gestiti di PyLv sono stati abilitati."

#: plconfig/cog.py:309
msgid "PyLav's managed node auto updates have been disabled."
msgstr "Gli aggiornamenti automatici del nodo gestito da PyLavè stato disabilitato."

#: plconfig/cog.py:317
#, docstring
msgid "Toggle the managed external node on/off."
msgstr "Attiva/disattiva il nodo esterno gestito."

#: plconfig/cog.py:335
msgid "PyLav's managed external node has been enabled."
msgstr "Il nodo esterno gestito da PyLavè stato abilitato."

#: plconfig/cog.py:343
msgid "PyLav's managed external node has been disabled."
msgstr "Il nodo esterno gestito da PyLavè stato disabilitato."

