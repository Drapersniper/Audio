#: mpnotifier/notifier.py:134
msgid "Track Stuck Event"
msgstr "Εκδήλωση Κομματιού Stuck"

#: mpnotifier/notifier.py:135
msgid "[Node={node}] {track} is stuck for {threshold} seconds, skipping."
msgstr "[Κόμβος={node}] {track} έχει κολλήσει για {threshold} δευτερόλεπτα, παραλείποντας."

#: mpnotifier/notifier.py:157
msgid "Track Exception Event"
msgstr "Παρακολούθηση Συμβάντος Εξαίρεσης"

#: mpnotifier/notifier.py:158
msgid "[Node={node}] There was an error while playing {track}:\n"
"{exception}"
msgstr "[Κόμβος={node}] Υπήρξε σφάλμα κατά την αναπαραγωγή {track}:\n"
"{exception}"

#: mpnotifier/notifier.py:176
msgid "the player reached the end of the tracks runtime."
msgstr "ο παίκτης έφτασε στο τέλος του χρόνου εκτέλεσης των κομματιών."

#: mpnotifier/notifier.py:178
msgid "a new track started playing."
msgstr "ένα νέο κομμάτι άρχισε να παίζει."

#: mpnotifier/notifier.py:180
msgid "it failed to start."
msgstr "απέτυχε να ξεκινήσει."

#: mpnotifier/notifier.py:182
msgid "because the player was stopped."
msgstr "επειδή ο παίκτης σταμάτησε."

#: mpnotifier/notifier.py:184
msgid "the node told it to stop."
msgstr "ο κόμβος του είπε να σταματήσει."

#: mpnotifier/notifier.py:187
msgid "Track End Event"
msgstr "Παρακολούθηση Τελικού Γεγονότος"

#: mpnotifier/notifier.py:188
msgid "[Node={node}] {track} has finished playing because {reason}"
msgstr "[Κόμβος={node}] {track} ολοκλήρωσε την αναπαραγωγή επειδή {reason}"

