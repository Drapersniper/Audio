#: mpnotifier/notifier.py:846
msgid "Track Skipped Event"
msgstr "Пропущенное событие трека"

#: mpnotifier/notifier.py:847
msgid "{track} has been skipped.\n"
"Requested by {requester}"
msgstr "{track} пропущен.\n"
"Запрошено {requester}"

#: mpnotifier/notifier.py:870
msgid "Track Seek Event"
msgstr "Событие Трек Seek"

#: mpnotifier/notifier.py:871
msgid "{requester} requested that {track} is sought from position {fro} to position {after}."
msgstr "{requester} запрашивает {track} с позиции {fro} на должность {after}."

#: mpnotifier/notifier.py:900
msgid "Track Previous Requested Event"
msgstr "Отслеживать предыдущее запрошенное событие"

#: mpnotifier/notifier.py:901
msgid "{requester} requested that the previous track {track} be played."
msgstr "{requester} просит воспроизвести предыдущий трек {track}."

#: mpnotifier/notifier.py:926
msgid "Tracks Requested Event"
msgstr "Треки запрошенного события"

#: mpnotifier/notifier.py:927
msgid "{requester} added {track_count} to the queue."
msgstr "{requester} добавил {track_count} в очередь."

#: mpnotifier/notifier.py:946
msgid "Track AutoPlay Event"
msgstr "Автовоспроизведение события"

#: mpnotifier/notifier.py:947
msgid "Auto-playing {track}."
msgstr "Авто-воспроизведение {track}."

#: mpnotifier/notifier.py:971
msgid "Track Resumed Event"
msgstr "Отслеживать возобновленное событие"

#: mpnotifier/notifier.py:972
msgid "{requester} resumed {track}."
msgstr "{requester} возобновил {track}."

#: mpnotifier/notifier.py:996
msgid "Queue Shuffled Event"
msgstr "Событие в очереди перемешивание"

#: mpnotifier/notifier.py:997
msgid "{requester} shuffled the queue."
msgstr "{requester} перемешивает очередь."

#: mpnotifier/notifier.py:1012
msgid "Queue End Event"
msgstr "Событие окончания очереди"

#: mpnotifier/notifier.py:1013
msgid "All tracks in the queue have been played."
msgstr "Все треки в очереди были воспроизведены."

#: mpnotifier/notifier.py:1035
msgid "Tracks Removed Event"
msgstr "Событие удаления треков"

#: mpnotifier/notifier.py:1036
msgid "{requester} removed {track_count} tracks from the queue."
msgstr "{requester} удалил {track_count} треков из очереди."

#: mpnotifier/notifier.py:1060
msgid "Player Paused Event"
msgstr "Игрок приостановлен"

#: mpnotifier/notifier.py:1061
msgid "{requester} paused the player."
msgstr "{requester} приостановил игрока."

#: mpnotifier/notifier.py:1083
msgid "Player Stopped Event"
msgstr "Событие остановки игрока"

#: mpnotifier/notifier.py:1084
msgid "{requester} stopped the player."
msgstr "{requester} остановил игрока."

#: mpnotifier/notifier.py:1106
msgid "Player Resumed Event"
msgstr "Плеер возобновил событие"

#: mpnotifier/notifier.py:1107
msgid "{requester} resumed the player."
msgstr "{requester} возобновил игрока."

#: mpnotifier/notifier.py:1129
msgid "Player Moved Event"
msgstr "Событие Перемещено"

#: mpnotifier/notifier.py:1130
msgid "{requester} moved the player from {before} to {after}."
msgstr "{requester} переместил игрока с {before} на {after}."

#: mpnotifier/notifier.py:1154
msgid "Player Disconnected Event"
msgstr "Событие отключения игрока"

#: mpnotifier/notifier.py:1155
msgid "{requester} disconnected the player."
msgstr "{requester} отключил игрока."

#: mpnotifier/notifier.py:1177
msgid "Player Connected Event"
msgstr "Подключено к событию"

#: mpnotifier/notifier.py:1178
msgid "{requester} connected the player."
msgstr "{requester} присоединился к игроку."

#: mpnotifier/notifier.py:1200
msgid "Player Volume Changed Event"
msgstr "Событие громкости игрока изменено"

#: mpnotifier/notifier.py:1201
msgid "{requester} changed the player's volume from {before} to {after}."
msgstr "{requester} изменил громкость игрока с {before} на {after}."

#: mpnotifier/notifier.py:1227 mpnotifier/notifier.py:1235
#: mpnotifier/notifier.py:1245
msgid "Player Repeat Event"
msgstr "Повторить событие"

#: mpnotifier/notifier.py:1228
msgid "{requester} disabled repeat."
msgstr "{requester} отключил повторение."

#: mpnotifier/notifier.py:1236
msgid "{requester} {status} repeat of the whole queue."
msgstr "{requester} {status} повторение всей очереди."

#: mpnotifier/notifier.py:1237 mpnotifier/notifier.py:1248
msgid "enabled"
msgstr "включен"

#: mpnotifier/notifier.py:1237 mpnotifier/notifier.py:1248
msgid "disabled"
msgstr "отключено"

#: mpnotifier/notifier.py:1246
msgid "{requester} {status} repeat for {track}."
msgstr "{requester} {status} повторить для {track}."

#: mpnotifier/notifier.py:1272
msgid "Player Restored Event"
msgstr "Восстановлено событие игрока"

#: mpnotifier/notifier.py:1273
msgid "{requester} restored the player."
msgstr "{requester} восстановил игрока."

#: mpnotifier/notifier.py:1291
msgid "an intro section"
msgstr "вводный раздел"

#: mpnotifier/notifier.py:1293
msgid "an outro section"
msgstr "outro раздел"

#: mpnotifier/notifier.py:1295
msgid "a preview section"
msgstr "секция предварительного просмотра"

#: mpnotifier/notifier.py:1297
msgid "an off-topic section"
msgstr "секция вне темы"

#: mpnotifier/notifier.py:1299
msgid "a filler section"
msgstr "секция наполнителя"

#: mpnotifier/notifier.py:1301
msgid "a sponsor section"
msgstr "раздел спонсора"

#: mpnotifier/notifier.py:1303
msgid "a self-promotion section"
msgstr "секция саморекламы"

#: mpnotifier/notifier.py:1305
msgid "an interaction section"
msgstr "секция взаимодействия"

#: mpnotifier/notifier.py:1309
msgid "Sponsor Segment Skipped Event"
msgstr "Сегмент спонсора пропущено событие"

#: mpnotifier/notifier.py:1310
msgid "Sponsorblock: Skipped {category} running from {start}s to {to}s."
msgstr "Спонсорблок: Пропущен {category} запущен с {start}до {to}с."

#: mpnotifier/notifier.py:1332
msgid "Effect"
msgstr "Эффект"

#: mpnotifier/notifier.py:1333
msgid "Values"
msgstr "Значения"

#: mpnotifier/notifier.py:1362
msgid "N/A"
msgstr "Н/Д"

#: mpnotifier/notifier.py:1366
msgid "Filters Applied Event"
msgstr "Фильтры примененного события"

#: mpnotifier/notifier.py:1367
msgid "[Node={node}] {requester} changed the player filters.\n\n"
"__**Currently Applied:**__\n"
"{data}"
msgstr "[Node={node}] {requester} изменил фильтров игрока.\n\n"
"__**Текущее применение:**__\n"
"{data}"

#: mpnotifier/notifier.py:1371
msgid "None"
msgstr "Нет"

#: mpnotifier/notifier.py:1387
msgid "Node Connected Event"
msgstr "Подключенное событие"

#: mpnotifier/notifier.py:1388
msgid "Node {name} has been connected."
msgstr "Узел {name} был подключен."

#: mpnotifier/notifier.py:1402
msgid "Node Disconnected Event"
msgstr "Событие отключения узла"

#: mpnotifier/notifier.py:1403
msgid "Node {name} has been disconnected with code {code} and reason: {reason}."
msgstr "Узел {name} был отключен с кодом {code} и причина: {reason}."

#: mpnotifier/notifier.py:1422
msgid "Node Changed Event"
msgstr "Событие изменения узла"

#: mpnotifier/notifier.py:1423
msgid "The node which the player is connected to changed from {fro} to {to}."
msgstr "Узел, к которому подключен игрок, изменился с {fro} на {to}."

#: mpnotifier/notifier.py:1442
msgid "WebSocket Closed Event"
msgstr "Событие закрытия WebSocket"

#: mpnotifier/notifier.py:1443
msgid "[Node={node}] The websocket connection to the Lavalink node closed with code {code} and reason {reason}."
msgstr "[Node={node}] Соединение веб-сокета с узлом Lavalink закрыто с кодом {code} и причиной {reason}."

#: mpnotifier/notifier_commands.py:81
#, docstring
msgid "Configure the MPNotifier cog."
msgstr "Настройте MPNotifier cog."

#: mpnotifier/notifier_commands.py:87
#, docstring
msgid "Set the notify channel for the player."
msgstr "Установить канал уведомления для плеера."

#: mpnotifier/notifier_commands.py:103
msgid "MP notify channel set to {channel}"
msgstr "Канал уведомления MP установлен в {channel}"

#: mpnotifier/notifier_commands.py:112
#, docstring
msgid "Set whether or not to notify for the specified event.\n\n"
"        Arguments:\n"
"            event -- The event to set.\n"
"            toggle -- Whether or not to notify upon receiving this event.\n"
"            use_mention -- Whether or not to use a mention instead of the name for the action requested.\n"
"        "
msgstr "Определяет, уведомлять ли об указанном событии.\n\n"
"        Аргументы:\n"
"            событие -- Событие для установки.\n"
"            включить/выключить -- Уведомлять ли об этом событии.\n"
"            use_mention -- Нужно ли использовать упоминание вместо имени запрошенного действия.\n"
"        "

#: mpnotifier/notifier_commands.py:127
msgid "Invalid event, possible events are:\n\n"
"{events}."
msgstr "Недопустимое событие, возможные события:\n\n"
"{events}."

#: mpnotifier/notifier_commands.py:141
msgid "Event {event} set to {toggle}{extras}."
msgstr "Событие {event} установлено в {toggle}{extras}."

