#: mpnotifier/notifier.py:870
msgid "Track Seek Event"
msgstr "Kappaleen Etsii Tapahtuma"

#: mpnotifier/notifier.py:871
msgid "{requester} requested that {track} is sought from position {fro} to position {after}."
msgstr "{requester} pyysi, että {track} etsitään asemasta {fro} paikkaan {after}."

#: mpnotifier/notifier.py:900
msgid "Track Previous Requested Event"
msgstr "Kappaleen Edellinen Pyydetty Tapahtuma"

#: mpnotifier/notifier.py:901
msgid "{requester} requested that the previous track {track} be played."
msgstr "{requester} pyysi, että edellinen kappale {track} soitettaisiin."

#: mpnotifier/notifier.py:926
msgid "Tracks Requested Event"
msgstr "Pyydetty Tapahtuma"

#: mpnotifier/notifier.py:927
msgid "{requester} added {track_count} to the queue."
msgstr "{requester} lisäsi {track_count} jonoon."

#: mpnotifier/notifier.py:946
msgid "Track AutoPlay Event"
msgstr "Kappaleen AutoPlay-tapahtuma"

#: mpnotifier/notifier.py:947
msgid "Auto-playing {track}."
msgstr "Näytetään automaattisesti {track}."

#: mpnotifier/notifier.py:971
msgid "Track Resumed Event"
msgstr "Kappaleen Alusta"

#: mpnotifier/notifier.py:972
msgid "{requester} resumed {track}."
msgstr "{requester} jatkettiin {track}."

#: mpnotifier/notifier.py:996
msgid "Queue Shuffled Event"
msgstr "Sekoitettu Jono"

#: mpnotifier/notifier.py:997
msgid "{requester} shuffled the queue."
msgstr "{requester} sekoitti jonon."

#: mpnotifier/notifier.py:1012
msgid "Queue End Event"
msgstr "Jonon Päättymistapahtuma"

#: mpnotifier/notifier.py:1013
msgid "All tracks in the queue have been played."
msgstr "Kaikki jonossa olevat kappaleet on soitettu."

#: mpnotifier/notifier.py:1035
msgid "Tracks Removed Event"
msgstr "Kappaleet Poistettu Tapahtuma"

#: mpnotifier/notifier.py:1036
msgid "{requester} removed {track_count} tracks from the queue."
msgstr "{requester} poisti {track_count} kappaletta jonosta."

#: mpnotifier/notifier.py:1060
msgid "Player Paused Event"
msgstr "Pelaaja Keskeytetty Tapahtuma"

#: mpnotifier/notifier.py:1061
msgid "{requester} paused the player."
msgstr "{requester} keskeytti pelaamisen."

#: mpnotifier/notifier.py:1083
msgid "Player Stopped Event"
msgstr "Soitin Pysäytetty Tapahtuma"

#: mpnotifier/notifier.py:1084
msgid "{requester} stopped the player."
msgstr "{requester} pysäytti pelaajan."

#: mpnotifier/notifier.py:1106
msgid "Player Resumed Event"
msgstr "Soitin Jatkosi Tapahtumaa"

#: mpnotifier/notifier.py:1107
msgid "{requester} resumed the player."
msgstr "{requester} jatkoi pelaajaa."

#: mpnotifier/notifier.py:1129
msgid "Player Moved Event"
msgstr "Pelaaja Siirretty Tapahtuma"

#: mpnotifier/notifier.py:1130
msgid "{requester} moved the player from {before} to {after}."
msgstr "{requester} siirsi pelaajan {before} kohteeseen {after}."

#: mpnotifier/notifier.py:1154
msgid "Player Disconnected Event"
msgstr "Pelaajan Yhteys Katkaistu"

#: mpnotifier/notifier.py:1155
msgid "{requester} disconnected the player."
msgstr "{requester} katkaisi yhteyden pelaajaan."

#: mpnotifier/notifier.py:1177
msgid "Player Connected Event"
msgstr "Soittimen Yhdistetty Tapahtuma"

#: mpnotifier/notifier.py:1178
msgid "{requester} connected the player."
msgstr "{requester} yhdisti pelaajan."

#: mpnotifier/notifier.py:1200
msgid "Player Volume Changed Event"
msgstr "Soittimen Äänenvoimakkuuden Muutettu Tapahtuma"

#: mpnotifier/notifier.py:1201
msgid "{requester} changed the player's volume from {before} to {after}."
msgstr "{requester} muutti pelaajan äänenvoimakkuuden {before} arvoon {after}."

#: mpnotifier/notifier.py:1227 mpnotifier/notifier.py:1235
#: mpnotifier/notifier.py:1245
msgid "Player Repeat Event"
msgstr "Toista Tapahtuma"

#: mpnotifier/notifier.py:1228
msgid "{requester} disabled repeat."
msgstr "{requester} poistettiin toiston käytöstä."

#: mpnotifier/notifier.py:1236
msgid "{requester} {status} repeat of the whole queue."
msgstr "{requester} {status} toista koko jono."

