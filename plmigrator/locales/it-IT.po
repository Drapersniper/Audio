#: plmigrator/cog.py:33
#, docstring
msgid "Copy Red's Audio settings over to PyLav"
msgstr "Copia le impostazioni dell'audio rosso su PyLav"

#: plmigrator/cog.py:44
#, docstring
msgid "Migrate Audio settings to PyLav\n\n"
"        Please note that this will replace any config already in PyLav.\n\n"
"        If you are sure you want to proceed please run this command again with the confirm argument set to 1\n"
"        i.e `[p]pylavmigrate 1`\n"
"        "
msgstr "Migrare le impostazioni audio in PyLav\n\n"
"        Si prega di notare che questo sostituirà qualsiasi configurazione già in PyLav.\n\n"
"        Se sei sicuro di voler procedere esegui nuovamente questo comando con l'argomento di conferma impostato a 1\n"
"        i. `[p]pylavmigrate 1`\n"
"        "

#: plmigrator/cog.py:111
msgid "I don't have permission to access {folder}"
msgstr "Non ho i permessi per accedere a {folder}"

#: plmigrator/cog.py:198
msgid "Migration of Audio cog settings to PyLav complete. Restart the bot for it to take effect.\n"
"{requester}"
msgstr "Migrazione delle impostazioni di Cog Audio a PyLav completa. Riavvia il bot perché abbia effetto.\n"
"{requester}"

