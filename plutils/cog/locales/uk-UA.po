#: plutils/cog/utility_commands.py:27
#, docstring
msgid "Utility commands for the MediaPlayer cog."
msgstr "Команди корисності для кога MediaPlay."

#: plutils/cog/utility_commands.py:32
#, docstring
msgid "Get info about specific things."
msgstr "Отримайте інформацію про конкретні речі."

#: plutils/cog/utility_commands.py:36
#, docstring
msgid "Get the base64 of the current track."
msgstr "Отримати base64 з поточного треку."

#: plutils/cog/utility_commands.py:44 plutils/cog/utility_commands.py:76
#: plutils/cog/utility_commands.py:108 plutils/cog/utility_commands.py:139
msgid "Not connected to a voice channel."
msgstr "Немає підключення до голосового каналу."

#: plutils/cog/utility_commands.py:52 plutils/cog/utility_commands.py:84
#: plutils/cog/utility_commands.py:116 plutils/cog/utility_commands.py:147
msgid "Nothing playing."
msgstr "Нічого не грає."

#: plutils/cog/utility_commands.py:67
#, docstring
msgid "Get the author of the current track."
msgstr "Отримати автора поточної композиції."

#: plutils/cog/utility_commands.py:99
#, docstring
msgid "Get the title of the current track."
msgstr "Отримати назву поточної композиції."

#: plutils/cog/utility_commands.py:131
#, docstring
msgid "Get the source of the current track."
msgstr "Отримати джерело поточного треку."

#: plutils/cog/utility_commands.py:162
#, docstring
msgid "Decode a tracks base64 string into a JSON object."
msgstr "Розширюйте рядок маршрутів base64 у JSON об'єкт."

#: plutils/cog/utility_commands.py:173
msgid "Invalid base64 string."
msgstr "Некоректний рядок base64."

#: plutils/cog/utility_commands.py:189
#, docstring
msgid "Manage the query cache."
msgstr "Керуйте кешем запиту."

#: plutils/cog/utility_commands.py:193
#, docstring
msgid "Clear the query cache."
msgstr "Очистити кеш запиту."

#: plutils/cog/utility_commands.py:200 plutils/cog/utility_commands.py:213
#: plutils/cog/utility_commands.py:226
msgid "Query cache cleared."
msgstr "Кеш запитів очищено."

#: plutils/cog/utility_commands.py:206
#, docstring
msgid "Clear the query cache older than a number of days."
msgstr "Очистити кеш запитів старше декількох днів."

#: plutils/cog/utility_commands.py:219
#, docstring
msgid "Clear the query cache for a query."
msgstr "Очистити кеш запитів для запиту."

#: plutils/cog/utility_commands.py:232
#, docstring
msgid "Get the size of the query cache."
msgstr "Отримати розмір кешу запиту."

#: plutils/cog/utility_commands.py:239
msgid "Query cache size: `{size}`."
msgstr "Розмір кешу запиту: `{size}`."

