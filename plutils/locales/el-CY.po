#: plutils/cog.py:25
#, docstring
msgid "Utility commands for PyLav."
msgstr "Εντολές χρησιμότητας για το PyLav."

#: plutils/cog.py:35
#, docstring
msgid "Utility commands for the MediaPlayer cog."
msgstr "Εντολές χρησιμότητας για τον κώδικα του MediaPlayer."

#: plutils/cog.py:40
#, docstring
msgid "Get info about specific things."
msgstr "Λάβετε πληροφορίες σχετικά με συγκεκριμένα πράγματα."

#: plutils/cog.py:44
#, docstring
msgid "Get the base64 of the current track."
msgstr "Αποκτήστε το base64 του τρέχοντος κομματιού."

#: plutils/cog.py:52 plutils/cog.py:84 plutils/cog.py:116 plutils/cog.py:147
msgid "Not connected to a voice channel."
msgstr "Δεν έχει συνδεθεί σε ένα κανάλι φωνής."

#: plutils/cog.py:60 plutils/cog.py:92 plutils/cog.py:124 plutils/cog.py:155
msgid "Nothing playing."
msgstr "Τίποτα δεν παίζει."

#: plutils/cog.py:75
#, docstring
msgid "Get the author of the current track."
msgstr "Λήψη του συγγραφέα του τρέχοντος κομματιού."

#: plutils/cog.py:107
#, docstring
msgid "Get the title of the current track."
msgstr "Λήψη τίτλου του τρέχοντος κομματιού."

#: plutils/cog.py:139
#, docstring
msgid "Get the source of the current track."
msgstr "Λήψη πηγής του τρέχοντος κομματιού."

#: plutils/cog.py:170
#, docstring
msgid "Decode a tracks base64 string into a JSON object."
msgstr "Αποκωδικοποίηση μιας συμβολοσειράς base64 σε ένα αντικείμενο JSON."

#: plutils/cog.py:181
msgid "Invalid base64 string."
msgstr "Μη έγκυρη συμβολοσειρά base64."

#: plutils/cog.py:198
#, docstring
msgid "Manage the query cache."
msgstr "Διαχείριση της λανθάνουσας μνήμης ερωτήματος."

#: plutils/cog.py:202
#, docstring
msgid "Clear the query cache."
msgstr "Εκκαθάριση της προσωρινής μνήμης ερωτήματος."

#: plutils/cog.py:209 plutils/cog.py:222 plutils/cog.py:235
msgid "Query cache cleared."
msgstr "Έγινε εκκαθάριση της λανθάνουσας μνήμης ερωτήματος."

#: plutils/cog.py:215
#, docstring
msgid "Clear the query cache older than a number of days."
msgstr "Εκκαθάριση της προσωρινής μνήμης ερωτημάτων που είναι παλαιότερη από μια σειρά ημερών."

#: plutils/cog.py:228
#, docstring
msgid "Clear the query cache for a query."
msgstr "Εκκαθάριση της προσωρινής μνήμης ερωτημάτων για ένα ερώτημα."

#: plutils/cog.py:241
#, docstring
msgid "Get the size of the query cache."
msgstr "Αποκτήστε το μέγεθος της λανθάνουσας μνήμης ερωτήματος."

#: plutils/cog.py:248
msgid "Query cache size: `{size}`."
msgstr "Μέγεθος λανθάνουσας μνήμης: `{size}`."

