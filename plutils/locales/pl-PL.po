#: plutils/cog.py:30 plutils/cog.py:40
#, docstring
msgid "Utility commands for PyLav."
msgstr "Polecenia użyteczności dla PyLav."

#: plutils/cog.py:44
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Pokaż wersję Cog i jego zależności PyLav."

#: plutils/cog.py:57
msgid "Library/Cog"
msgstr "Biblioteka/Cog"

#: plutils/cog.py:57
msgid "Version"
msgstr "Wersja"

#: plutils/cog.py:65
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via Code block on Discord channel.\n"
"        "
msgstr "Pokaż ukośniki dostępne w botze.\n\n"
"        Autor: TrustyJAID#0001 poprzez blok kodu na kanale Discord.\n"
"        "

#: plutils/cog.py:96
#, docstring
msgid "Get info about specific things."
msgstr "Uzyskaj informacje na temat konkretnych rzeczy."

#: plutils/cog.py:100
#, docstring
msgid "Get the base64 of the current track."
msgstr "Zdobądź base64 bieżącego utworu."

#: plutils/cog.py:108 plutils/cog.py:140 plutils/cog.py:172 plutils/cog.py:203
msgid "Not connected to a voice channel."
msgstr "Nie podłączono do kanału głosowego."

#: plutils/cog.py:116 plutils/cog.py:148 plutils/cog.py:180 plutils/cog.py:211
msgid "Nothing playing."
msgstr "Nic nie gra."

#: plutils/cog.py:131
#, docstring
msgid "Get the author of the current track."
msgstr "Pobierz autora bieżącego utworu."

#: plutils/cog.py:163
#, docstring
msgid "Get the title of the current track."
msgstr "Pobierz tytuł bieżącego utworu."

#: plutils/cog.py:195
#, docstring
msgid "Get the source of the current track."
msgstr "Pobierz źródło bieżącego utworu."

#: plutils/cog.py:226
#, docstring
msgid "Decode a tracks base64 string into a JSON object."
msgstr "Dekoduj ciąg base64 do obiektu JSON."

#: plutils/cog.py:237
msgid "Invalid base64 string."
msgstr "Nieprawidłowy ciąg base64."

#: plutils/cog.py:254
#, docstring
msgid "Manage the query cache."
msgstr "Zarządzaj pamięcią podręczną kwerendy."

#: plutils/cog.py:258
#, docstring
msgid "Clear the query cache."
msgstr "Wyczyść pamięć podręczną kwerendy."

#: plutils/cog.py:265 plutils/cog.py:294 plutils/cog.py:307
msgid "Query cache cleared."
msgstr "Pamięć podręczna zapytania wyczyszczona."

#: plutils/cog.py:271
#, docstring
msgid "Clear the query cache older than a number of days."
msgstr "Wyczyść pamięć podręczną zapytania starszą niż kilka dni."

#: plutils/cog.py:279
msgid "Days must be less than 31."
msgstr "Dni muszą być mniejsze niż 31."

#: plutils/cog.py:287
msgid "Days must be greater than 1."
msgstr "Dni muszą być większe niż 1."

#: plutils/cog.py:300
#, docstring
msgid "Clear the query cache for a query."
msgstr "Wyczyść pamięć podręczną dla zapytania."

#: plutils/cog.py:313
#, docstring
msgid "Get the size of the query cache."
msgstr "Uzyskaj rozmiar pamięci podręcznej zapytania."

#: plutils/cog.py:320
msgid "Query cache size: `{size}`."
msgstr "Rozmiar pamięci podręcznej zapytania: `{size}`."

