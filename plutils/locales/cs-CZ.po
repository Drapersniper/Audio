msgstr "Neplatný řetězec base64"

#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "Správa mezipaměti dotazů"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "Vymazání mezipaměti dotazů"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "Mezipaměť dotazů byla vymazána"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Vymažte mezipaměť dotazů starší než několik dní"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "Počet dní musí být kratší než 31"

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "Dny musí být delší než 1"

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "Vymazání mezipaměti dotazů pro dotaz"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "Získání velikosti mezipaměti dotazů"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "Velikost mezipaměti dotazu: '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Spuštění trasování paměti\n\n"
"        `[p]plutils trace 0' vypne trasování\n"
"        `[p]plutils trace 1' zapne trasování\n"
"        `[p]plutils trace\" zobrazuje aktuální stav trasování\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "Zastavené trasování paměti"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "Spuštění trasování paměti"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "Nejprve musíte začít trasovat"

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Nastavení úrovně protokolovacího nástroje\n\n"
"        Úrovně jsou následující:\n"
"        0: Kritický\n"
"        1: Chyba\n"
"        2: Upozornění\n"
"        3: Informace\n"
"        4: Ladění\n"
"        5: Podrobné\n"
"        6: Trasování\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "Úroveň záznamníku nastavená na '{level}`"

