#: plutils/cog.py:30 plutils/cog.py:40
#, docstring
msgid "Utility commands for PyLav."
msgstr "Příkazy nástrojů pro PyLav."

#: plutils/cog.py:44
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Ukaž verzi Cog a jsou to PyLav závislosti."

#: plutils/cog.py:57
msgid "Library/Cog"
msgstr "Knihovna/Cog"

#: plutils/cog.py:57
msgid "Version"
msgstr "Verze"

#: plutils/cog.py:65
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via Code block on Discord channel.\n"
"        "
msgstr "Zobrazit lomítka v botu.\n\n"
"        Autor: TrustyJAID#0001 prostřednictvím kódu bloku na Discord kanálu\n"
"        "

#: plutils/cog.py:96
#, docstring
msgid "Get info about specific things."
msgstr "Získejte informace o konkrétních věcech."

#: plutils/cog.py:100
#, docstring
msgid "Get the base64 of the current track."
msgstr "Získejte základnu 64 aktuální trasy."

#: plutils/cog.py:108 plutils/cog.py:140 plutils/cog.py:172 plutils/cog.py:203
msgid "Not connected to a voice channel."
msgstr "Nepřipojeno k hlasovému kanálu."

#: plutils/cog.py:116 plutils/cog.py:148 plutils/cog.py:180 plutils/cog.py:211
msgid "Nothing playing."
msgstr "Nic nehraje."

#: plutils/cog.py:131
#, docstring
msgid "Get the author of the current track."
msgstr "Získejte autora aktuální skladby."

#: plutils/cog.py:163
#, docstring
msgid "Get the title of the current track."
msgstr "Získejte název aktuální skladby."

#: plutils/cog.py:195
#, docstring
msgid "Get the source of the current track."
msgstr "Získejte zdroj aktuální skladby."

#: plutils/cog.py:226
#, docstring
msgid "Decode a tracks base64 string into a JSON object."
msgstr "Dekódujte základní řetězec skladeb na JSON objekt."

#: plutils/cog.py:237
msgid "Invalid base64 string."
msgstr "Neplatný řetězec base64."

#: plutils/cog.py:254
#, docstring
msgid "Manage the query cache."
msgstr "Správa mezipaměti dotazů."

#: plutils/cog.py:258
#, docstring
msgid "Clear the query cache."
msgstr "Vymazat mezipaměť dotazu."

#: plutils/cog.py:265 plutils/cog.py:294 plutils/cog.py:307
msgid "Query cache cleared."
msgstr "Mezipaměť dotazů vymazána."

#: plutils/cog.py:271
#, docstring
msgid "Clear the query cache older than a number of days."
msgstr "Vymazat mezipaměť dotazů starší než několik dní."

#: plutils/cog.py:279
msgid "Days must be less than 31."
msgstr "Dny musí být kratší než 31."

#: plutils/cog.py:287
msgid "Days must be greater than 1."
msgstr "Dny musí být delší než 1."

#: plutils/cog.py:300
#, docstring
msgid "Clear the query cache for a query."
msgstr "Vymazat mezipaměť dotazů pro dotaz."

#: plutils/cog.py:313
#, docstring
msgid "Get the size of the query cache."
msgstr "Získej velikost mezipaměti dotazu."

#: plutils/cog.py:320
msgid "Query cache size: `{size}`."
msgstr "Vyhledávací keše: `{size}`."

