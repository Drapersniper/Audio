msgstr "Není připojen k hlasovému kanálu"

#: plutils/cog.py:153 plutils/cog.py:185 plutils/cog.py:217 plutils/cog.py:248
msgid "Nothing playing"
msgstr "Nic nehraje"

#: plutils/cog.py:168
#, docstring
msgid "Get the author of the current track"
msgstr "Získání autora aktuální skladby"

#: plutils/cog.py:200
#, docstring
