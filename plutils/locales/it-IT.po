#: plutils/cog.py:30 plutils/cog.py:40
#, docstring
msgid "Utility commands for PyLav."
msgstr "Comandi di utilità per PyLav."

#: plutils/cog.py:44
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Mostra la versione del Cog ed è le dipendenze di PyLAV."

#: plutils/cog.py:57
msgid "Library/Cog"
msgstr "Library/Cog"

#: plutils/cog.py:57
msgid "Version"
msgstr "Versione"

#: plutils/cog.py:65
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via Code block on Discord channel.\n"
