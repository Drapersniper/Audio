#, docstring
msgid "Utility commands for PyLav."
msgstr "Comenzi de utilitate pentru PyLav."

#: plutils/cog.py:44
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Arată versiunea Cog şi este dependenţe PyLav."

#: plutils/cog.py:57
msgid "Library/Cog"
msgstr "Bibliotecă/Cog"

#: plutils/cog.py:57
msgid "Version"
msgstr "Versiune"

