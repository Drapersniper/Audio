msgid "Utility commands for PyLav."
msgstr "أوامر مفيدة لبايلاف."

#: plutils/cog.py:44
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "إظهار نسخة من الكتلة وهي تبعيات PyLav ."

#: plutils/cog.py:57
msgid "Library/Cog"
msgstr "المكتبة/الكلب"

#: plutils/cog.py:57
msgid "Version"
msgstr "الإصدار"

#: plutils/cog.py:65
#, docstring
msgid "Show the slashes available in the bot.\n\n"
