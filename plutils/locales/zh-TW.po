#: plutils/cog.py:60 plutils/cog.py:70
#, docstring
msgid "Utility commands for PyLav"
msgstr "PyLav 的實用命令"

#: plutils/cog.py:74
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "顯示 Cog 的版本及其 PyLav 依賴項目"

#: plutils/cog.py:91
msgid "Library/Cog"
msgstr "資料庫/Cog"

#: plutils/cog.py:92
msgid "Version"
msgstr "版本"

#: plutils/cog.py:105
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "顯示機器人中可用的斜杠。\n\n"
"        作者：TrustyJAID#0001 通過 Discord 頻道上的代碼。\n"
"        "

#: plutils/cog.py:136
#, docstring
msgid "Get info about specific things"
msgstr "獲取有關特定事物的資訊"

#: plutils/cog.py:140
#, docstring
msgid "Get the base64 of the current track"
msgstr "獲取當前曲目的base64"

#: plutils/cog.py:148 plutils/cog.py:180 plutils/cog.py:212 plutils/cog.py:243
msgid "Not connected to a voice channel"
msgstr "未連接至語音頻道"

#: plutils/cog.py:156 plutils/cog.py:188 plutils/cog.py:220 plutils/cog.py:251
msgid "Nothing playing"
msgstr "沒有正在播放的曲目"

#: plutils/cog.py:171
#, docstring
msgid "Get the author of the current track"
msgstr "獲取當前曲目的作者"

#: plutils/cog.py:203
#, docstring
msgid "Get the title of the current track"
msgstr "獲取當前曲目的標題"

#: plutils/cog.py:235
#, docstring
msgid "Get the source of the current track"
msgstr "獲取當前曲目的來源"

#: plutils/cog.py:266
#, docstring
msgid "Decode a track's base64 string into a JSON object"
msgstr "將軌道的 base64 字符串解碼為 JSON 對象"

#: plutils/cog.py:277
msgid "Invalid base64 string"
msgstr "無效的 base64 字符串"

#: plutils/cog.py:294
#, docstring
msgid "Manage the query cache"
msgstr "查詢託管點緩存"

#: plutils/cog.py:298
#, docstring
msgid "Clear the query cache"
msgstr "清除查詢緩存"

#: plutils/cog.py:305 plutils/cog.py:334 plutils/cog.py:347
msgid "Query cache cleared"
msgstr "查詢緩存已清除"

#: plutils/cog.py:311
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "清除早於一定天數的查詢緩存"

#: plutils/cog.py:319
msgid "Days must be less than 31"
msgstr "天數必須小於 31"

#: plutils/cog.py:327
msgid "Days must be greater than 1"
msgstr "天數必須大於 1"

#: plutils/cog.py:340
#, docstring
msgid "Clear the query cache for a query"
msgstr "清除查詢的查詢緩存"

#: plutils/cog.py:353
#, docstring
msgid "Get the size of the query cache"
msgstr "獲取查詢緩存的大小"

#: plutils/cog.py:360
msgid "Query cache size: `{size}`"
msgstr "查詢緩存大小：`{size}`"

#: plutils/cog.py:371
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "開始追蹤記憶體\n\n"
"        `[p]plutils trace 0` 關閉跟蹤\n"
"        `[p]plutils trace 1` 打開跟蹤\n"
"        `[p]plutils trace` 顯示跟蹤的當前狀態\n"
"        "

#: plutils/cog.py:385
msgid "Stopped memory tracing"
msgstr "停止追蹤記憶體"

#: plutils/cog.py:394
msgid "Started memory tracing"
msgstr "開始追蹤記憶體"

#: plutils/cog.py:403
msgid "You need to start tracing first"
msgstr "您需要先開始追蹤"

#: plutils/cog.py:415
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "設置記錄器級別\n\n"
"        等級如下：\n"
"        0：嚴重\n"
"        1：錯誤\n"
"        2：警告\n"
"        3：信息\n"
"        4：除錯\n"
"        5：冗長\n"
"        6：跟蹤\n"
"        "

#: plutils/cog.py:446
msgid "Logger level set to `{level}`"
msgstr "記錄器級別設置為“{level}”"

