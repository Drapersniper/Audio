#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "管理查詢快取"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "清除查詢快取"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "查詢快取已清除"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "清除超過天數的查詢緩存"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "天數必須少於31天"

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "天數必須大於 1"

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "清除查詢的查詢緩存"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "獲取查詢快取的大小"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "查詢快取大小： '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "啟動記憶體跟蹤\n\n"
"        `[p]Plutils 追蹤 0' 關閉追蹤\n"
"        `[p]Plutils Track 1' 打開跟蹤\n"
"        `[p]Plutils Trace'顯示跟蹤的當前狀態\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "已停止記憶體跟蹤"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "已啟動記憶體跟蹤"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "您需要先開始跟蹤"

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "設置記錄器級別\n\n"
"        級別如下：\n"
"        0：嚴重\n"
"        1：錯誤\n"
"        2：警告\n"
"        3：資訊\n"
"        4：調試\n"
"        5：詳細\n"
"        6：跟蹤\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "記錄器等級設定為 '{level}`"

