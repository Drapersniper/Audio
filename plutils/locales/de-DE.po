msgstr "Ungültige base64-Zeichenfolge"

#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "Verwalten des Abfragecaches"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "Löschen des Abfragecaches"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "Abfragecache gelöscht"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Löschen des Abfragecaches, der älter als eine Anzahl von Tagen ist"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "Tage müssen weniger als 31 betragen"

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "Tage müssen größer als 1 sein"

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "Löschen des Abfragecaches für eine Abfrage"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "Abrufen der Größe des Abfragecaches"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "Größe des Abfragecaches: '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Starten der Speicherablaufverfolgung\n\n"
"        `[p]plutils trace 0' deaktiviert die Ablaufverfolgung\n"
"        `[p]plutils trace 1' aktiviert die Ablaufverfolgung\n"
"        `[p]plutils trace' zeigt den aktuellen Status des Tracing an\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "Speicherablaufverfolgung beendet"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "Speicherablaufverfolgung gestartet"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "Sie müssen zuerst mit der Ablaufverfolgung beginnen"

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Stellen Sie den Logger-Level ein\n\n"
"        Die folgenden Stufen sind die folgenden:\n"
"        0: Kritisch\n"
"        1: Fehler\n"
"        2: Warnung\n"
"        3: Info\n"
"        4: Debuggen\n"
"        5: Ausführlich\n"
"        6: Ablaufverfolgung\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "Logger-Level auf '{level}`"

