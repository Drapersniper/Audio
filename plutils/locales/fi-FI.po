#: plutils/cog.py:25 plutils/cog.py:35
#, docstring
msgid "Utility commands for PyLav."
msgstr "Hyödyllisyyskomennot PyLavia varten."

#: plutils/cog.py:40
#, docstring
msgid "Get info about specific things."
msgstr "Saat tietoa tietyistä asioista."

#: plutils/cog.py:44
#, docstring
msgid "Get the base64 of the current track."
msgstr "Hanki nykyisen radan perus64."

#: plutils/cog.py:52 plutils/cog.py:84 plutils/cog.py:116 plutils/cog.py:147
msgid "Not connected to a voice channel."
msgstr "Ei ole yhdistetty äänikanavaan."

#: plutils/cog.py:60 plutils/cog.py:92 plutils/cog.py:124 plutils/cog.py:155
msgid "Nothing playing."
msgstr "Ei mitään toistettavaa."

#: plutils/cog.py:75
#, docstring
msgid "Get the author of the current track."
msgstr "Hanki nykyisen kappaleen tekijä."

#: plutils/cog.py:107
#, docstring
msgid "Get the title of the current track."
msgstr "Hanki nykyisen kappaleen otsikko."

#: plutils/cog.py:139
#, docstring
msgid "Get the source of the current track."
msgstr "Hanki nykyisen radan lähde."

#: plutils/cog.py:170
#, docstring
msgid "Decode a tracks base64 string into a JSON object."
msgstr "Purkaa kappaleiden base64 merkkijono JSON objektiin."

#: plutils/cog.py:181
msgid "Invalid base64 string."
msgstr "Virheellinen base64-merkkijono."

#: plutils/cog.py:198
#, docstring
msgid "Manage the query cache."
msgstr "Hallitse kyselyn välimuistia."

#: plutils/cog.py:202
#, docstring
msgid "Clear the query cache."
msgstr "Tyhjennä kyselyn välimuisti."

#: plutils/cog.py:209 plutils/cog.py:222 plutils/cog.py:235
msgid "Query cache cleared."
msgstr "Kyselyn välimuisti tyhjennetty."

#: plutils/cog.py:215
#, docstring
msgid "Clear the query cache older than a number of days."
msgstr "Tyhjennä kyselyn välimuisti joka on vanhempi kuin päivien määrä."

#: plutils/cog.py:228
#, docstring
msgid "Clear the query cache for a query."
msgstr "Tyhjennä kyselyn välimuisti kyselyä varten."

#: plutils/cog.py:241
#, docstring
msgid "Get the size of the query cache."
msgstr "Hanki kyselyn koko välimuisti."

#: plutils/cog.py:248
msgid "Query cache size: `{size}`."
msgstr "Kyselyn välimuistin koko: `{size}`."

