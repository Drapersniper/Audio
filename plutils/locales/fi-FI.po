#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "PyLavin apuohjelmakomennot"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Näytä hammaspyörän versio ja sen PyLav-riippuvuudet"

#: plutils/cog.py:88
msgid "Library/Cog"
msgstr "Kirjasto/hammasratas"

#: plutils/cog.py:89
msgid "Version"
msgstr "Versio"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Näytä botissa käytettävissä olevat vinoviivat.\n\n"
"        Kirjoittaja: TrustyJAID # 0001 Discord-kanavan koodilohkon kautta.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Hanki tietoja tietyistä asioista"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Hanki nykyisen raidan perusta64"

#: plutils/cog.py:145 plutils/cog.py:177 plutils/cog.py:209 plutils/cog.py:240
#: plutils/cog.py:273
msgid "Not connected to a voice channel"
msgstr "Ei ole yhteydessä äänikanavaan"

#: plutils/cog.py:153 plutils/cog.py:185 plutils/cog.py:217 plutils/cog.py:248
msgid "Nothing playing"
msgstr "Mikään ei pelaa"

#: plutils/cog.py:168
#, docstring
msgid "Get the author of the current track"
msgstr "Hanki nykyisen raidan tekijä"

#: plutils/cog.py:200
#, docstring
msgid "Get the title of the current track"
msgstr "Hanki nykyisen kappaleen nimi"

#: plutils/cog.py:232
#, docstring
msgid "Get the source of the current track"
msgstr "Hanki nykyisen raidan lähde"

#: plutils/cog.py:264
#, docstring
msgid "Get the API of the current track"
msgstr "Hanki nykyisen raidan API"

#: plutils/cog.py:284 plutils/cog.py:293
msgid "Unable to get player info"
msgstr "Pelaajatietoja ei voi saada"

#: plutils/cog.py:309
#, docstring
msgid "Decode a track's base64 string into a JSON object"
msgstr "Raidan base64-merkkijonon purkaminen JSON-objektiksi"

#: plutils/cog.py:318
msgid "Invalid base64 string"
msgstr "Virheellinen base64-merkkijono"

#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "Kyselyn välimuistin hallinta"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "Tyhjennä kyselyn välimuisti"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "Kyselyn välimuisti tyhjennetty"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Tyhjennä kyselyn välimuisti, joka on vanhempi kuin useita päiviä"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "Päivien on oltava alle 31"

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "Päivien on oltava suurempia kuin 1"

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "Kyselyn välimuistin tyhjentäminen"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "Hae kyselyn välimuistin koko"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "Kyselyvälimuistin koko: '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Aloita muistin jäljitys\n\n"
"        `[p]plutils trace 0' sammuttaa jäljityksen\n"
"        `[p]plutils jäljittää 1' kytkeytyy jäljitykseen\n"
"        `[p]plutils trace\" näyttää jäljityksen nykytilan\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "Pysäytetty muistin jäljitys"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "Aloitti muistin jäljityksen"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "Sinun on aloitettava jäljitys ensin"

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Loggerin tason asettaminen\n\n"
"        Tasot ovat seuraavat:\n"
"        0: Kriittinen\n"
"        1: Virhe\n"
"        2: Varoitus\n"
"        3: Tiedot\n"
"        4: Virheenkorjaus\n"
"        5: Monisanainen\n"
"        6: Jäljitä\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "Loggerin tasoksi on asetettu '{level}`"

