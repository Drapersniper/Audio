#: plutils/cog.py:25 plutils/cog.py:35
#, docstring
msgid "Utility commands for PyLav."
msgstr "Verktygskommandon för PyLav."

#: plutils/cog.py:40
#, docstring
msgid "Get info about specific things."
msgstr "Få information om specifika saker."

#: plutils/cog.py:44
#, docstring
msgid "Get the base64 of the current track."
msgstr "Hämta base64 för den aktuella banan."

#: plutils/cog.py:52 plutils/cog.py:84 plutils/cog.py:116 plutils/cog.py:147
msgid "Not connected to a voice channel."
msgstr "Inte ansluten till en röstkanal."

#: plutils/cog.py:60 plutils/cog.py:92 plutils/cog.py:124 plutils/cog.py:155
msgid "Nothing playing."
msgstr "Ingenting spelar."

#: plutils/cog.py:75
#, docstring
msgid "Get the author of the current track."
msgstr "Hämta författaren till den aktuella låten."

#: plutils/cog.py:107
#, docstring
msgid "Get the title of the current track."
msgstr "Få titeln på den aktuella låten."

#: plutils/cog.py:139
#, docstring
msgid "Get the source of the current track."
msgstr "Få källan till den aktuella spåret."

#: plutils/cog.py:170
#, docstring
msgid "Decode a tracks base64 string into a JSON object."
msgstr "Avkoda ett spår base64-sträng till ett JSON-objekt."

#: plutils/cog.py:181
msgid "Invalid base64 string."
msgstr "Ogiltig base64-sträng."

#: plutils/cog.py:198
#, docstring
msgid "Manage the query cache."
msgstr "Hantera frågecachen."

#: plutils/cog.py:202
#, docstring
msgid "Clear the query cache."
msgstr "Rensa cachen för sökfrågan."

#: plutils/cog.py:209 plutils/cog.py:222 plutils/cog.py:235
msgid "Query cache cleared."
msgstr "Frågecache rensad."

#: plutils/cog.py:215
#, docstring
msgid "Clear the query cache older than a number of days."
msgstr "Rensa cachen äldre än ett antal dagar."

#: plutils/cog.py:228
#, docstring
msgid "Clear the query cache for a query."
msgstr "Rensa frågecache för en fråga."

#: plutils/cog.py:241
#, docstring
msgid "Get the size of the query cache."
msgstr "Få storleken på frågecachen."

#: plutils/cog.py:248
msgid "Query cache size: `{size}`."
msgstr "Frågecache storlek: `{size}`."

