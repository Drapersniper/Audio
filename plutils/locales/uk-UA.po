#: plutils/cog.py:30 plutils/cog.py:40
#, docstring
msgid "Utility commands for PyLav."
msgstr "Команди для PyLav."

#: plutils/cog.py:44
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Покажіть версію Кога і це — піЛавові залежності."

#: plutils/cog.py:57
msgid "Library/Cog"
msgstr "Бібліотека/Ког"

#: plutils/cog.py:57
msgid "Version"
msgstr "Версія"

#: plutils/cog.py:65
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via Code block on Discord channel.\n"
"        "
msgstr "Показати коші, доступні в боті.\n\n"
"        Автор: TrustyJAID#0001 через блок коду на каналі Discord\n"
"        "

#: plutils/cog.py:96
#, docstring
msgid "Get info about specific things."
msgstr "Отримайте інформацію про конкретні речі."

#: plutils/cog.py:100
#, docstring
msgid "Get the base64 of the current track."
msgstr "Отримати base64 з поточного треку."

#: plutils/cog.py:108 plutils/cog.py:140 plutils/cog.py:172 plutils/cog.py:203
msgid "Not connected to a voice channel."
msgstr "Немає підключення до голосового каналу."

#: plutils/cog.py:116 plutils/cog.py:148 plutils/cog.py:180 plutils/cog.py:211
msgid "Nothing playing."
msgstr "Нічого не грає."

#: plutils/cog.py:131
#, docstring
msgid "Get the author of the current track."
msgstr "Отримати автора поточної композиції."

#: plutils/cog.py:163
#, docstring
msgid "Get the title of the current track."
msgstr "Отримати назву поточної композиції."

#: plutils/cog.py:195
#, docstring
msgid "Get the source of the current track."
msgstr "Отримати джерело поточного треку."

#: plutils/cog.py:226
#, docstring
msgid "Decode a tracks base64 string into a JSON object."
msgstr "Розширюйте рядок маршрутів base64 у JSON об'єкт."

#: plutils/cog.py:237
msgid "Invalid base64 string."
msgstr "Некоректний рядок base64."

#: plutils/cog.py:254
#, docstring
msgid "Manage the query cache."
msgstr "Керуйте кешем запиту."

#: plutils/cog.py:258
#, docstring
msgid "Clear the query cache."
msgstr "Очистити кеш запиту."

#: plutils/cog.py:265 plutils/cog.py:294 plutils/cog.py:307
msgid "Query cache cleared."
msgstr "Кеш запитів очищено."

#: plutils/cog.py:271
#, docstring
msgid "Clear the query cache older than a number of days."
msgstr "Очистити кеш запитів старше декількох днів."

#: plutils/cog.py:279
msgid "Days must be less than 31."
msgstr "Днів має бути менше 31."

#: plutils/cog.py:287
msgid "Days must be greater than 1."
msgstr "Дні має бути більше 1."

#: plutils/cog.py:300
#, docstring
msgid "Clear the query cache for a query."
msgstr "Очистити кеш запитів для запиту."

#: plutils/cog.py:313
#, docstring
msgid "Get the size of the query cache."
msgstr "Отримати розмір кешу запиту."

#: plutils/cog.py:320
msgid "Query cache size: `{size}`."
msgstr "Розмір кешу запиту: `{size}`."

