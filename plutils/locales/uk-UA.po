#: plutils/cog.py:58 plutils/cog.py:68
#, docstring
msgid "Utility commands for PyLav"
msgstr "Команди утиліти для PyLav"

#: plutils/cog.py:72
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Показати версію гвинтика і його залежностей PyLav"

#: plutils/cog.py:88
msgid "Library/Cog"
msgstr "Бібліотека/гвинтик"

#: plutils/cog.py:89
msgid "Version"
msgstr "Версія"

#: plutils/cog.py:102
#, docstring
msgid "Show the slashes available in the bot.\n\n"
"        Author: TrustyJAID#0001 via code block on Discord channel.\n"
"        "
msgstr "Покажіть скісні риски, наявні в боті.\n\n"
"        Автор: TrustyJAID#0001 через блок коду на каналі Discord.\n"
"        "

#: plutils/cog.py:133
#, docstring
msgid "Get info about specific things"
msgstr "Отримання відомостей про певні речі"

#: plutils/cog.py:137
#, docstring
msgid "Get the base64 of the current track"
msgstr "Отримайте базу64 поточного треку"

#: plutils/cog.py:145 plutils/cog.py:177 plutils/cog.py:209 plutils/cog.py:240
#: plutils/cog.py:273
msgid "Not connected to a voice channel"
msgstr "Не підключено до голосового каналу"

#: plutils/cog.py:153 plutils/cog.py:185 plutils/cog.py:217 plutils/cog.py:248
msgid "Nothing playing"
msgstr "Нічого не грає"

#: plutils/cog.py:168
#, docstring
msgid "Get the author of the current track"
msgstr "Отримати автора актуального треку"

#: plutils/cog.py:200
#, docstring
msgid "Get the title of the current track"
msgstr "Отримайте назву поточного треку"

#: plutils/cog.py:232
#, docstring
msgid "Get the source of the current track"
msgstr "Отримати джерело поточного треку"

#: plutils/cog.py:264
#, docstring
msgid "Get the API of the current track"
msgstr "Отримати API поточного треку"

#: plutils/cog.py:284 plutils/cog.py:293
msgid "Unable to get player info"
msgstr "Не вдається отримати інформацію про програвач"

#: plutils/cog.py:309
#, docstring
msgid "Decode a track's base64 string into a JSON object"
msgstr "Декодування рядка base64 треку в об'єкт JSON"

#: plutils/cog.py:318
msgid "Invalid base64 string"
msgstr "Неприпустимий рядок base64"

#: plutils/cog.py:334
#, docstring
msgid "Manage the query cache"
msgstr "Керування кешем запиту"

#: plutils/cog.py:338
#, docstring
msgid "Clear the query cache"
msgstr "Очищення кешу запиту"

#: plutils/cog.py:345 plutils/cog.py:374 plutils/cog.py:387
msgid "Query cache cleared"
msgstr "Кеш запиту очищено"

#: plutils/cog.py:351
#, docstring
msgid "Clear the query cache older than a number of days"
msgstr "Очищення кеша запиту, старішого за кілька днів"

#: plutils/cog.py:359
msgid "Days must be less than 31"
msgstr "Днів має бути менше 31"

#: plutils/cog.py:367
msgid "Days must be greater than 1"
msgstr "Днів має бути більше 1"

#: plutils/cog.py:380
#, docstring
msgid "Clear the query cache for a query"
msgstr "Очищення кешу запиту для запиту"

#: plutils/cog.py:393
#, docstring
msgid "Get the size of the query cache"
msgstr "Отримання розміру кешу запиту"

#: plutils/cog.py:400
msgid "Query cache size: `{size}`"
msgstr "Розмір кешу запиту: '{size}`"

#: plutils/cog.py:411
#, docstring
msgid "Start memory tracing\n\n"
"        `[p]plutils trace 0` turns off tracing\n"
"        `[p]plutils trace 1` turns on tracing\n"
"        `[p]plutils trace` shows the current status of tracing\n"
"        "
msgstr "Початок трасування пам'яті\n\n"
"        `[p]Трасування Plutils 0' вимикає трасування\n"
"        `[p]Plutils Trace 1' включає трасування\n"
"        `[p]plutils trace' показує поточний стан трасування\n"
"        "

#: plutils/cog.py:425
msgid "Stopped memory tracing"
msgstr "Зупинене трасування пам'яті"

#: plutils/cog.py:434
msgid "Started memory tracing"
msgstr "Розпочато трасування пам'яті"

#: plutils/cog.py:443
msgid "You need to start tracing first"
msgstr "Спочатку потрібно почати трасування"

#: plutils/cog.py:455
#, docstring
msgid "Set the logger level\n\n"
"        Levels are the following:\n"
"        0: Critical\n"
"        1: Error\n"
"        2: Warning\n"
"        3: Info\n"
"        4: Debug\n"
"        5: Verbose\n"
"        6: Trace\n"
"        "
msgstr "Встановіть рівень лісоруба\n\n"
"        Рівні такі:\n"
"        0: Критичний\n"
"        1: Помилка\n"
"        2: Попередження\n"
"        3: Інформація\n"
"        4: Налагодження\n"
"        5: Багатослівність\n"
"        6: Слід\n"
"        "

#: plutils/cog.py:486
msgid "Logger level set to `{level}`"
msgstr "Рівень логгера встановлено на '{level}`"

