#: plnotifier/cog.py:83
#, docstring
msgid "Listen to events from the PyLav player and sent them as messages to the specified channel."
msgstr "Słuchaj wydarzeń od gracza PyLav i wysłał je jako wiadomości do określonego kanału."

#: plnotifier/cog.py:195
#, docstring
msgid "Configure the PyLavNotifier cog."
msgstr "Skonfiguruj cog PyLavNotifier"

#: plnotifier/cog.py:199
#, docstring
msgid "Show the version of the Cog and it's PyLav dependencies."
msgstr "Pokaż wersję Cog i jego zależności PyLav."

#: plnotifier/cog.py:212
msgid "Library/Cog"
msgstr "Biblioteka/Cog"

#: plnotifier/cog.py:212
msgid "Version"
msgstr "Wersja"

#: plnotifier/cog.py:222
#, docstring
msgid "Set the notify channel for the player."
msgstr "Ustaw kanał powiadomień dla gracza."

#: plnotifier/cog.py:238
msgid "PyLavNotifier channel set to {channel}"
msgstr "Kanał PyLavNotifier ustawiony na {channel}"

#: plnotifier/cog.py:248
#, docstring
msgid "Set whether or not to notify for the specified event.\n\n"
"        Arguments:\n"
"            event -- The event to set.\n"
"            toggle -- Whether or not to notify upon receiving this event.\n"
"            use_mention -- Whether or not to use a mention instead of the name for the action requested.\n"
"        "
msgstr "Ustaw czy chcesz powiadomić o określonym wydarzeniu.\n\n"
"        Argumenty:\n"
"            zdarzenie -- Wydarzenie do ustawiania.\n"
"            przełącz -- Czy powiadomić czy nie otrzymasz tego wydarzenia.\n"
"            użyj wzmianki -- niezależnie od tego, czy użyć wzmianki zamiast nazwy żądanej akcji.\n"
"        "

#: plnotifier/cog.py:263
msgid "Invalid event, possible events are:\n\n"
"{events}."
msgstr "Nieprawidłowe wydarzenie, możliwe zdarzenia to:\n\n"
"{events}."

