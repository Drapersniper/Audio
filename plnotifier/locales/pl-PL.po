#: plnotifier/cog.py:84
#, docstring
msgid "Listen to events from the PyLav player and send them as messages to the specified channel"
msgstr "Słuchaj wydarzeń z odtwarzacza PyLav i wysyłaj je jako wiadomości do określonego kanału"

#: plnotifier/cog.py:227
#, docstring
msgid "Configure the PyLavNotifier cog"
msgstr "Konfigurowanie koła zębatego PyLavNotifier"

#: plnotifier/cog.py:231
#, docstring
msgid "Show the version of the Cog and its PyLav dependencies"
msgstr "Pokaż wersję trybika i jego zależności PyLav"

#: plnotifier/cog.py:247
msgid "Library/Cog"
msgstr "Biblioteka/Trybik"

#: plnotifier/cog.py:248
msgid "Version"
msgstr "Wersja"

#: plnotifier/cog.py:263
#, docstring
msgid "Set the notify channel for the player"
msgstr "Ustawianie kanału powiadamiania dla odtwarzacza"

#: plnotifier/cog.py:272 plnotifier/cog.py:327
msgid "I do not have permission to manage webhooks in {channel}"
msgstr "Nie mam uprawnień do zarządzania elementami webhook w {channel}"

#: plnotifier/cog.py:286
msgid "I do not have permission to create a thread in {channel}"
msgstr "Nie mam uprawnień do tworzenia wątku w {channel}"

#: plnotifier/cog.py:295 plnotifier/cog.py:311 plnotifier/cog.py:336
msgid "PyLavNotifier"
msgstr "PyLavNotifier"

#: plnotifier/cog.py:296 plnotifier/cog.py:314 plnotifier/cog.py:337
msgid "PyLav Notifier - Requested by {author}"
msgstr "PyLav Notifier - Na życzenie {author}"

#: plnotifier/cog.py:307
msgid "This thread will be used by PyLav to post notifications about the player"
msgstr "Ten wątek będzie używany przez PyLav do publikowania powiadomień o graczu"

#: plnotifier/cog.py:351
msgid "PyLavNotifier channel set to {channel}"
msgstr "Kanał PyLavNotifier ustawiony na {channel}"

#: plnotifier/cog.py:361
#, docstring
msgid "Set whether or not to notify for the specified event.\n\n"
"        Arguments:\n"
"            event -- The event to set.\n"
"            toggle -- Whether or not to notify upon receiving this event.\n"
"            use_mention -- Whether or not to use a mention instead of the name for the action requested.\n"
"        "
msgstr "Określ, czy chcesz powiadamiać o określonym zdarzeniu.\n\n"
"        Argumenty:\n"
"            event -- zdarzenie do ustawienia.\n"
"            przełącz -- czy powiadomić o tym zdarzeniu.\n"
"            use_mention -- Czy zamiast nazwy żądanej akcji ma być użyta wzmianka.\n"
"        "

#: plnotifier/cog.py:378
